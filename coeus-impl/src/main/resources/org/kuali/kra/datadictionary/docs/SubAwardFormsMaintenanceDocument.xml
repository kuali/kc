<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="SubAwardFormsMaintenanceDocument" parent="SubAwardFormsMaintenanceDocument-parentBean" />
    <bean id="SubAwardFormsMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.kra.subaward.bo.SubAwardForms" />
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
        <property name="maintainableSections" >
            <list>
                <ref bean="SubAwardFormsMaintenanceDocument-EditSubAwardForms" />
            </list>
        </property>
        <property name="lockingKeys" >
            <list>
                <value>formId</value>
            </list>
        </property>
        <property name="allowsRecordDeletion" value="true" />
        <property name="documentTypeName" value="SubAwardFormsMaintenanceDocument" />
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
        <property name="businessRulesClass" value="org.kuali.kra.subaward.subawardrule.SubAwardFormsMaintenanceDocumentRule"/>
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="SubAwardFormsMaintenanceDocument-EditSubAwardForms" parent="SubAwardFormsMaintenanceDocument-EditSubAwardForms-parentBean" />
    <bean id="SubAwardFormsMaintenanceDocument-EditSubAwardForms-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit SubAwardForms Lookup" />
        <property name="title" value="Edit SubAwardForms Lookup" />
        <property name="maintainableItems" >
            <list>            
                <bean parent="MaintainableFieldDefinition"
                    p:name="formId" 
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="description"
                    p:required="true" /> 				
                <bean parent="MaintainableFieldDefinition" 
                    p:name="templateFile"
                    p:required="true"/>
                <bean parent="MaintainableFieldDefinition" 
                    p:name="templateTypeCode"
                    p:required="true"/>
            </list>
        </property>
    </bean>
</beans>
