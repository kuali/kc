              <bean p:propertyName="protocolActionTypeCode" parent="Uif-DataField"/>
              <bean p:propertyName="actionNumber" parent="Uif-DataField"/>
              <bean p:propertyName="followupActionCode" parent="Uif-DataField"/>
                        </list>
