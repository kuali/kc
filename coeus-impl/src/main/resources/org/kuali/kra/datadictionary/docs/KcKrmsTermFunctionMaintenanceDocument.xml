<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="KcKrmsTermFunctionMaintenanceDocument" parent="KcKrmsTermFunctionMaintenanceDocument-parentBean" />
    <bean id="KcKrmsTermFunctionMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.common.framework.krms.KcKrmsTermFunction" />
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
        <property name="maintainableSections" >
            <list>
                <ref bean="KcKrmsTermFunctionMaintenanceDocument-EditKcKrmsTermFunction" />
            </list>
        </property>
        <property name="lockingKeys" >
            <list>
                <value>kcKrmsTermFunctionId</value>
            </list>
        </property>
        <property name="documentTypeName" value="KcKrmsTermFunctionMaintenanceDocument" />
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase" />
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="KcKrmsTermFunctionMaintenanceDocument-EditKcKrmsTermFunction" parent="KcKrmsTermFunctionMaintenanceDocument-EditKcKrmsTermFunction-parentBean" />
    <bean id="KcKrmsTermFunctionMaintenanceDocument-EditKcKrmsTermFunction-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Kc Krms Term Function" />
        <property name="title" value="Edit Kc Krms Term Function" />
        <property name="maintainableItems" >
            <list>
                <bean parent="MaintainableFieldDefinition"
                    p:name="kcKrmsTermFunctionId"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="krmsTermName" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="description"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="returnType"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="functionType" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
        </property>
    </bean>
</beans>
