<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="IacucProtocolFundingSource" parent="IacucProtocolFundingSource-parentBean" />
	<bean id="IacucProtocolFundingSource-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.iacuc.protocol.funding.IacucProtocolFundingSource" />
		<property name="objectLabel" value="Protocol Funding Source" />
		<property name="inquiryDefinition" >
			<ref bean="IacucProtocolFundingSource-inquiryDefinition" />
		</property>
		<property name="lookupDefinition" >
			<ref bean="IacucProtocolFundingSource-lookupDefinition" />
		</property>
		<property name="helpDefinition" >
            <bean parent="HelpDefinition" p:parameterNamespace="KC-IACUC" p:parameterName="protocolFundingSourcesHelpUrl" p:parameterDetailType="Document" />
        </property>
		<property name="titleAttribute" value="protocolFundingSourceId" />
		<property name="attributes" >
			<list>
				<ref bean="IacucProtocolFundingSource-protocolFundingSourceId" />
				<ref bean="IacucProtocolFundingSource-protocolId" />
				<ref bean="IacucProtocolFundingSource-protocolNumber" />
				<ref bean="IacucProtocolFundingSource-sequenceNumber" />
				<ref bean="IacucProtocolFundingSource-fundingSourceTypeCode" />
                <ref bean="IacucProtocolFundingSource-fundingSourceNumber" />
                <ref bean="IacucProtocolFundingSource-fundingSourceName" />
                <ref bean="IacucProtocolFundingSource-fundingSourceTitle" />
				<ref bean="IacucProtocolFundingSource-versionNumber" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	<bean id="IacucProtocolFundingSource-protocolFundingSourceId" parent="IacucProtocolFundingSource-protocolFundingSourceId-parentBean" />
	<bean id="IacucProtocolFundingSource-protocolFundingSourceId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolFundingSourceId" />
		<property name="required" value="false" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Funding Source Id" />
		<property name="shortLabel" value="Funding Id" />
		<property name="maxLength" value="12" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
		<property name="control" >
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
    <property name="controlField">
      <bean p:size="12" parent="Uif-TextControl"/>
		</property>
		<property name="summary" value="Protocol Funding Source Id" />
		<property name="description" value="Protocol Funding Source Id" />
	</bean>

	<bean id="IacucProtocolFundingSource-protocolId" parent="IacucProtocolFundingSource-protocolId-parentBean" />
	<bean id="IacucProtocolFundingSource-protocolId-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolId" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Id" />
		<property name="shortLabel" value="Protocol Id" />
		<property name="maxLength" value="12" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
		<property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
		</property>
		<property name="summary" value="Protocol Id" />
		<property name="description" value="Protocol Id" />
	</bean>

	<bean id="IacucProtocolFundingSource-protocolNumber" parent="IacucProtocolFundingSource-protocolNumber-parentBean" />
	<bean id="IacucProtocolFundingSource-protocolNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="protocolNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Protocol Number" />
		<property name="shortLabel" value="Protocol Number" />
		<property name="maxLength" value="20" />
		<property name="validationPattern" >
			<bean parent="AlphaNumericValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
		<property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
		</property>
		<property name="summary" value="Protocol Number" />
		<property name="description" value="Protocol Number" />
	</bean>

	<bean id="IacucProtocolFundingSource-sequenceNumber" parent="IacucProtocolFundingSource-sequenceNumber-parentBean" />
	<bean id="IacucProtocolFundingSource-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="sequenceNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Sequence Number" />
		<property name="shortLabel" value="Sequence Number" />
		<property name="maxLength" value="4" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
		<property name="control" >
      <bean parent="TextControlDefinition" p:size="4"/>
    </property>
    <property name="controlField">
      <bean p:size="4" parent="Uif-TextControl"/>
		</property>
		<property name="summary" value="Sequence Number" />
		<property name="description" value="Sequence Number" />
	</bean>

	<bean id="IacucProtocolFundingSource-fundingSourceTypeCode" parent="IacucProtocolFundingSource-fundingSourceTypeCode-parentBean" />
	<bean id="IacucProtocolFundingSource-fundingSourceTypeCode-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="fundingSourceTypeCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Funding Type" />
		<property name="shortLabel" value="Funding Type" />
		<property name="maxLength" value="22" />
		<property name="validationPattern" >
			<bean parent="NumericValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="required" value="true" />
		<property name="control" >
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.kra.bo.FundingSourceType" p:valuesFinderClass="org.kuali.coeus.sys.framework.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:includeBlankRow="false" p:keyAttribute="fundingSourceTypeCode" p:labelAttribute="description"/>
    </property>
    <property name="optionsFinder">
      <bean class="org.kuali.coeus.sys.framework.keyvalue.ExtendedPersistableBusinessObjectValuesFinder">
        <property name="businessObjectClass" value="org.kuali.kra.bo.FundingSourceType"/>
        <property name="includeKeyInDescription" value="false"/>
        <property name="includeBlankRow" value="false"/>
        <property name="keyAttributeName" value="fundingSourceTypeCode"/>
        <property name="labelAttributeName" value="description"/>
      </bean>
    </property>
    <property name="controlField">
      <bean parent="Uif-DropdownControl"/>
        </property>
		<property name="summary" value="Funding Source Type Code" />
		<property name="description" value="Funding Source Type Code" />
	</bean>
	
	<bean id="IacucProtocolFundingSource-fundingSourceNumber" parent="IacucProtocolFundingSource-fundingSourceNumber-parentBean" />
	<bean id="IacucProtocolFundingSource-fundingSourceNumber-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="fundingSourceNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Funding Number" />
        <property name="shortLabel" value="Funding Number" />
        <property name="maxLength" value="200" />
	    <property name="control" >
      <bean parent="TextControlDefinition" p:size="21"/>
    </property>
    <property name="validationPattern" >
    	<bean parent="UTF8AnyCharacterValidationPattern" p:allowWhitespace="true" />
    </property>
    <property name="validCharactersConstraint">
    	<bean parent="UTF8AnyCharacterPatternConstraint">
        	<property name="allowWhitespace" value="true"/>
      	</bean>
    </property>
    <property name="controlField">
      <bean p:size="21" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Funding Source Number" />
        <property name="description" value="Funding Source Number" />
    </bean>

	<bean id="IacucProtocolFundingSource-versionNumber" parent="IacucProtocolFundingSource-versionNumber-parentBean" />
  <bean id="IacucProtocolFundingSource-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>
    <bean id="IacucProtocolFundingSource-fundingSourceName" parent="IacucProtocolFundingSource-fundingSourceName-parentBean" />
    <bean id="IacucProtocolFundingSource-fundingSourceName-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="fundingSourceName" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Source" />
        <property name="shortLabel" value="Source" />
        <property name="maxLength" value="200" />
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="21"/>
    </property>
    <property name="validationPattern" >
    	<bean parent="UTF8AnyCharacterValidationPattern" p:allowWhitespace="true" />
    </property>
    <property name="validCharactersConstraint">
    	<bean parent="UTF8AnyCharacterPatternConstraint">
        	<property name="allowWhitespace" value="true"/>
      	</bean>
    </property>
    <property name="controlField">
      <bean p:size="21" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Funding Source Name" />
        <property name="description" value="Funding Source Name" />
    </bean>

    <bean id="IacucProtocolFundingSource-fundingSourceTitle" parent="IacucProtocolFundingSource-fundingSourceTitle-parentBean" />
    <bean id="IacucProtocolFundingSource-fundingSourceTitle-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="fundingSourceTitle" />
        <property name="required" value="false" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Title" />
        <property name="shortLabel" value="Title" />
        <property name="maxLength" value="200" />
        <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
        </property>
        <property name="control" >
            <bean parent="HiddenControlDefinition"/>
        </property>
    <property name="controlField">
      <bean parent="Uif-HiddenControl"/>
    </property>
        <property name="summary" value="Funding Source Title" />
        <property name="description" value="Funding Source Title" />
    </bean>    

	<!-- Business Object Inquiry Definition -->
	<bean id="IacucProtocolFundingSource-inquiryDefinition" parent="IacucProtocolFundingSource-inquiryDefinition-parentBean" />
  <bean id="IacucProtocolFundingSource-InquiryView" parent="IacucProtocolFundingSource-InquiryView-parentBean"/>
	<bean id="IacucProtocolFundingSource-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
		<property name="title" value="Protocol Funding Source Id" />
		<property name="inquirySections" >
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Protocol Funding Source Id" />
					<property name="numberOfColumns" value="1" />
					<property name="inquiryFields" >
						<list>
              <bean parent="FieldDefinition" p:attributeName="protocolFundingSourceId" p:forceInquiry="true"/>
              <bean parent="FieldDefinition" p:attributeName="protocolId"/>
              <bean parent="FieldDefinition" p:attributeName="protocolNumber"/>
              <bean parent="FieldDefinition" p:attributeName="sequenceNumber"/>
              <bean parent="FieldDefinition" p:attributeName="fundingSourceTypeCode"/>
              <bean parent="FieldDefinition" p:attributeName="fundingSourceNumber"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="IacucProtocolFundingSource-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
    <property name="headerText" value="Protocol Funding Source Id"/>
    <property name="dataObjectClassName" value="org.kuali.kra.iacuc.protocol.funding.IacucProtocolFundingSource"/>
    <property name="items">
      <list>
        <bean parent="Uif-Disclosure-GridSection">
          <property name="headerText" value="Protocol Funding Source Id"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
              <bean p:propertyName="protocolFundingSourceId" parent="Uif-DataField"/>
              <bean p:propertyName="protocolId" parent="Uif-DataField"/>
              <bean p:propertyName="protocolNumber" parent="Uif-DataField"/>
              <bean p:propertyName="sequenceNumber" parent="Uif-DataField"/>
              <bean p:propertyName="fundingSourceTypeCode" parent="Uif-DataField"/>
              <bean p:propertyName="fundingSourceNumber" parent="Uif-DataField"/>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->
	<bean id="IacucProtocolFundingSource-lookupDefinition" parent="IacucProtocolFundingSource-lookupDefinition-parentBean" />
  <bean id="IacucProtocolFundingSource-LookupView" parent="IacucProtocolFundingSource-LookupView-parentBean"/>
	<bean id="IacucProtocolFundingSource-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
		<property name="title" value="Protocol Funding Source Lookup" /><property name="defaultSort" >
      <bean parent="SortDefinition"/>
		</property>
		<property name="lookupFields" >
			<list>
        <bean parent="FieldDefinition" p:attributeName="protocolFundingSourceId" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="protocolId"/>
        <bean parent="FieldDefinition" p:attributeName="protocolNumber"/>
        <bean parent="FieldDefinition" p:attributeName="sequenceNumber"/>
        <bean parent="FieldDefinition" p:attributeName="fundingSourceTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="fundingSourceNumber"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="protocolFundingSourceId" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="protocolId"/>
        <bean parent="FieldDefinition" p:attributeName="protocolNumber"/>
        <bean parent="FieldDefinition" p:attributeName="sequenceNumber"/>
        <bean parent="FieldDefinition" p:attributeName="fundingSourceTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="fundingSourceNumber"/>
      </list>
    </property>
  </bean>

  <bean id="IacucProtocolFundingSource-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
    <property name="dataObjectClassName" value="org.kuali.kra.iacuc.protocol.funding.IacucProtocolFundingSource"/>
    <property name="headerText" value="Protocol Funding Source Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="protocolFundingSourceId" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="protocolId" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="protocolNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="sequenceNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="fundingSourceTypeCode" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="fundingSourceNumber" parent="Uif-LookupCriteriaInputField"/>
			</list>
		</property>
		<property name="resultFields" >
			<list>
        <bean p:propertyName="protocolFundingSourceId" parent="Uif-DataField"/>
        <bean p:propertyName="protocolId" parent="Uif-DataField"/>
        <bean p:propertyName="protocolNumber" parent="Uif-DataField"/>
        <bean p:propertyName="sequenceNumber" parent="Uif-DataField"/>
        <bean p:propertyName="fundingSourceTypeCode" parent="Uif-DataField"/>
        <bean p:propertyName="fundingSourceNumber" parent="Uif-DataField"/>
			</list>
		</property>
	</bean>
</beans>
