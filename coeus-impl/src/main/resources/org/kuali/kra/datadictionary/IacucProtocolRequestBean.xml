<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="IacucProtocolRequestBean" parent="IacucProtocolRequestBean-parentBean" />
	<bean id="IacucProtocolRequestBean-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.kra.iacuc.actions.request.IacucProtocolRequestBean" />
		<property name="objectLabel" value="Iacuc Protocol Request Bean" />
		<property name="titleAttribute" value="IacucProtocolRequestBean-reason" />
		<property name="attributes" >
			<list>
				<ref bean="IacucProtocolRequestBean-reason" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
    <bean id="IacucProtocolRequestBean-reason" parent="IacucProtocolRequestBean-reason-parentBean" />
	<bean id="IacucProtocolRequestBean-reason-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="reason" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Reason" />
		<property name="shortLabel" value="Reason" />
		<property name="maxLength" value="2000" />
		<property name="control" >
      <bean parent="TextareaControlDefinition" p:rows="4" p:cols="100"/>
    </property>
    <property name="controlField">
      <bean p:rows="4" p:cols="100" parent="Uif-TextAreaControl"/>
		</property>
		<property name="summary" value="Reason" />
		<property name="description" value="Reason" />
	</bean>	

</beans>
