    <bean id="ProposalCustomData-proposalNumber" parent="ProposalCustomData-proposalNumber-parentBean" />
    <bean id="ProposalCustomData-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Number" />
        <property name="shortLabel" value="Proposal Number" />
        <property name="maxLength" value="10" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal Number" />
        <property name="description" value="Proposal Number" />
    </bean>

    <bean id="ProposalCustomData-sequenceNumber" parent="ProposalCustomData-sequenceNumber-parentBean" />
    <bean id="ProposalCustomData-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="sequenceNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Sequence Number" />
        <property name="shortLabel" value="Sequence Number" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Sequence Number" />
        <property name="description" value="Sequence Number" />
    </bean>

    <bean id="ProposalCustomData-customAttributeId" parent="ProposalCustomData-customAttributeId-parentBean" />
    <bean id="ProposalCustomData-customAttributeId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="customAttributeId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Custom Attribute Id" />
        <property name="shortLabel" value="Custom Attribute Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Custom Attribute Id" />
        <property name="description" value="Custom Attribute Id" />
    </bean>

    <bean id="ProposalCustomData-value" parent="ProposalCustomData-value-parentBean" />
    <bean id="ProposalCustomData-value-parentBean" abstract="true" parent="KraAttributeReferenceDummy-CustomDataValue" />

    <bean id="ProposalCustomData-versionNumber" parent="ProposalCustomData-versionNumber-parentBean" />
  <bean id="ProposalCustomData-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>
    <!-- Business Object Inquiry Definition -->
    <bean id="ProposalCustomData-inquiryDefinition" parent="ProposalCustomData-inquiryDefinition-parentBean" />
  <bean id="ProposalCustomData-InquiryView" parent="ProposalCustomData-InquiryView-parentBean"/>
    <bean id="ProposalCustomData-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Proposal Custom Data Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Proposal Custom Data Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
              <bean parent="FieldDefinition" p:attributeName="proposalCustomDataId" p:forceInquiry="true"/>
              <bean parent="FieldDefinition" p:attributeName="proposalNumber"/>
              <bean parent="FieldDefinition" p:attributeName="sequenceNumber"/>
              <bean parent="FieldDefinition" p:attributeName="customAttributeId"/>
              <bean parent="FieldDefinition" p:attributeName="value"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="ProposalCustomData-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
    <property name="headerText" value="Proposal Custom Data Id"/>
    <property name="dataObjectClassName" value="org.kuali.kra.institutionalproposal.customdata.InstitutionalProposalCustomData"/>
    <property name="items">
      <list>
        <bean parent="Uif-Disclosure-GridSection">
          <property name="headerText" value="Proposal Custom Data Id"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
              <bean p:propertyName="proposalCustomDataId" parent="Uif-DataField"/>
              <bean p:propertyName="proposalNumber" parent="Uif-DataField"/>
              <bean p:propertyName="sequenceNumber" parent="Uif-DataField"/>
              <bean p:propertyName="customAttributeId" parent="Uif-DataField"/>
              <bean p:propertyName="value" parent="Uif-DataField"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="ProposalCustomData-lookupDefinition" parent="ProposalCustomData-lookupDefinition-parentBean" />
  <bean id="ProposalCustomData-LookupView" parent="ProposalCustomData-LookupView-parentBean"/>
    <bean id="ProposalCustomData-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Proposal Custom Data Lookup" /><property name="defaultSort" >
      <bean parent="SortDefinition"/>
        </property>
        <property name="lookupFields" >
            <list>
        <bean parent="FieldDefinition" p:attributeName="proposalCustomDataId" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="proposalNumber"/>
        <bean parent="FieldDefinition" p:attributeName="sequenceNumber"/>
        <bean parent="FieldDefinition" p:attributeName="customAttributeId"/>
        <bean parent="FieldDefinition" p:attributeName="value"/>
            </list>
        </property>
        <property name="resultFields" >
            <list>
        <bean parent="FieldDefinition" p:attributeName="proposalCustomDataId" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="proposalNumber"/>
        <bean parent="FieldDefinition" p:attributeName="sequenceNumber"/>
        <bean parent="FieldDefinition" p:attributeName="customAttributeId"/>
        <bean parent="FieldDefinition" p:attributeName="value"/>
      </list>
    </property>
  </bean>

  <bean id="ProposalCustomData-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
    <property name="dataObjectClassName" value="org.kuali.kra.institutionalproposal.customdata.InstitutionalProposalCustomData"/>
    <property name="headerText" value="Proposal Custom Data Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="proposalCustomDataId" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="proposalNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="sequenceNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="customAttributeId" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="value" parent="Uif-LookupCriteriaInputField"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean p:propertyName="proposalCustomDataId" parent="Uif-DataField"/>
        <bean p:propertyName="proposalNumber" parent="Uif-DataField"/>
        <bean p:propertyName="sequenceNumber" parent="Uif-DataField"/>
        <bean p:propertyName="customAttributeId" parent="Uif-DataField"/>
        <bean p:propertyName="value" parent="Uif-DataField"/>
      </list>
    </property>
  </bean>

</beans>
