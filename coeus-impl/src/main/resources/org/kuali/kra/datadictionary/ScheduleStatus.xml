<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="ScheduleStatus" parent="ScheduleStatus-parentBean"/>
	<bean abstract="true" id="ScheduleStatus-parentBean" parent="BusinessObjectEntry">
		<property name="businessObjectClass" value="org.kuali.coeus.common.committee.impl.bo.ScheduleStatus"/>
		<property name="objectLabel" value="Schedule Status"/>
		<property name="inquiryDefinition">
			<ref bean="ScheduleStatus-inquiryDefinition"/>
		</property>
		<property name="lookupDefinition">
			<ref bean="ScheduleStatus-lookupDefinition"/>
		</property>
		<property name="helpDefinition" >
            <bean parent="HelpDefinition" p:parameterNamespace="KC-COMMITTEE" p:parameterName="meetingDetailsHelpUrl" p:parameterDetailType="Document" />
        </property>
		<property name="titleAttribute" value="scheduleStatusCode"/>
		<property name="attributes">
			<list>
				<ref bean="ScheduleStatus-scheduleStatusCode"/>
				<ref bean="ScheduleStatus-description"/>
				<ref bean="ScheduleStatus-versionNumber"/>
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->
	<bean id="ScheduleStatus-scheduleStatusCode" parent="ScheduleStatus-scheduleStatusCode-parentBean"/>
	<bean abstract="true" id="ScheduleStatus-scheduleStatusCode-parentBean" parent="AttributeDefinition">
		<property name="name" value="scheduleStatusCode"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Schedule Status Code"/>
		<property name="shortLabel" value="Schedule Status Code"/>
		<property name="maxLength" value="22"/>
		<property name="validationPattern">
			<bean parent="NumericValidationPattern"/>
		</property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
    </property>
		<property name="summary" value="Schedule Status Code"/>
		<property name="description" value="Schedule Status Code"/>
	</bean>

	<bean id="ScheduleStatus-description" parent="ScheduleStatus-description-parentBean"/>
	<bean abstract="true" id="ScheduleStatus-description-parentBean" parent="AttributeDefinition">
		<property name="name" value="description"/>
		<property name="forceUppercase" value="false"/>
		<property name="label" value="Description"/>
		<property name="shortLabel" value="Description"/>
		<property name="maxLength" value="200"/>
    <property name="validationPattern">
      <bean p:allowWhitespace="true" parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
    </property>
		<property name="control">
			<bean p:size="10" parent="TextControlDefinition"/>
		</property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
    </property>
		<property name="summary" value="Description"/>
		<property name="description" value="Description"/>
	</bean>

	<bean id="ScheduleStatus-versionNumber" parent="ScheduleStatus-versionNumber-parentBean"/>
  <bean abstract="true" id="ScheduleStatus-versionNumber-parentBean" parent="AttributeReferenceDummy-versionNumber"/>
	<!-- Business Object Inquiry Definition -->
	<bean id="ScheduleStatus-inquiryDefinition" parent="ScheduleStatus-inquiryDefinition-parentBean"/>
  <bean id="ScheduleStatus-InquiryView" parent="ScheduleStatus-InquiryView-parentBean"/>
	<bean abstract="true" id="ScheduleStatus-inquiryDefinition-parentBean" parent="InquiryDefinition">
		<property name="title" value="Schedule Status Code"/>
		<property name="inquirySections">
			<list>
				<bean parent="InquirySectionDefinition">
					<property name="title" value="Schedule Status Code"/>
					<property name="numberOfColumns" value="1"/>
					<property name="inquiryFields">
						<list>
							<bean p:attributeName="scheduleStatusCode" p:forceInquiry="true" parent="FieldDefinition"/>
							<bean p:attributeName="description" parent="FieldDefinition"/>
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>

  <bean id="ScheduleStatus-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
    <property name="headerText" value="Schedule Status Code"/>
    <property name="dataObjectClassName" value="org.kuali.coeus.common.committee.impl.bo.ScheduleStatus"/>
    <property name="items">
      <list>
        <bean parent="Uif-Disclosure-GridSection">
          <property name="headerText" value="Schedule Status Code"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
              <bean p:propertyName="scheduleStatusCode" parent="Uif-DataField"/>
              <bean p:propertyName="description" parent="Uif-DataField"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

	<!-- Business Object Lookup Definition -->
	<bean id="ScheduleStatus-lookupDefinition" parent="ScheduleStatus-lookupDefinition-parentBean"/>
  <bean id="ScheduleStatus-LookupView" parent="ScheduleStatus-LookupView-parentBean"/>
	<bean abstract="true" id="ScheduleStatus-lookupDefinition-parentBean" parent="LookupDefinition">
		<property name="title" value="Schedule Status Lookup"/><property name="defaultSort">
      <bean parent="SortDefinition"/>
		</property>
		<property name="lookupFields">
			<list>
				<bean p:attributeName="scheduleStatusCode" p:forceInquiry="true" parent="FieldDefinition"/>
				<bean p:attributeName="description" parent="FieldDefinition"/>
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean p:attributeName="scheduleStatusCode" p:forceInquiry="true" parent="FieldDefinition"/>
				<bean p:attributeName="description" parent="FieldDefinition"/>
			</list>
		</property>
	</bean>

  <bean id="ScheduleStatus-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
    <property name="dataObjectClassName" value="org.kuali.coeus.common.committee.impl.bo.ScheduleStatus"/>
    <property name="headerText" value="Schedule Status Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="scheduleStatusCode" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="description" parent="Uif-LookupCriteriaInputField"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean p:propertyName="scheduleStatusCode" parent="Uif-DataField"/>
        <bean p:propertyName="description" parent="Uif-DataField"/>
      </list>
    </property>
  </bean>

</beans>
