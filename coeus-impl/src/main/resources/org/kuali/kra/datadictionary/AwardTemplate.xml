		                    <bean p:attributeName="awardTemplateTermId" parent="FieldDefinition"/>
		                    <bean p:attributeName="sponsorTermTypeCode" parent="FieldDefinition"/>
		                    <bean p:attributeName="description" parent="FieldDefinition"/>
		                  </list>
		                </property>
