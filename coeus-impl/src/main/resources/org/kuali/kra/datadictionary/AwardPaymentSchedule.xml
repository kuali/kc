    <property name="headerText" value="Award Payment Schedule Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="awardPaymentScheduleId" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="awardNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="sequenceNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="dueDate" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="amount" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="submitDate" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="submittedBy" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="invoiceNumber" parent="Uif-LookupCriteriaInputField"/>
