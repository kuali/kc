      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean p:propertyName="awardPaymentScheduleId" parent="Uif-DataField"/>
        <bean p:propertyName="awardNumber" parent="Uif-DataField"/>
        <bean p:propertyName="sequenceNumber" parent="Uif-DataField"/>
        <bean p:propertyName="dueDate" parent="Uif-DataField"/>
        <bean p:propertyName="amount" parent="Uif-DataField"/>
        <bean p:propertyName="submitDate" parent="Uif-DataField"/>
        <bean p:propertyName="submittedBy" parent="Uif-DataField"/>
        <bean p:propertyName="invoiceNumber" parent="Uif-DataField"/>
        <bean p:propertyName="statusDescription" parent="Uif-DataField"/>
