			<xs:element name="sponsorName" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="templateStatusType">
		<xs:sequence>
			<xs:element name="statusCode" type="xs:int" minOccurs="0"/>
