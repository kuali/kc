        <property name="items">
            <list>
                <bean parent="PropDev-SubmitPage-Hierarchy" p:order="10"/>
            </list>
        </property>
        <property name="header">
            <bean parent="Uif-PageHeader" />
        </property>
    </bean>

</beans>
