                <bean parent="PropDev-SubmitPage-SupplementalInfo" p:order="70" />
                <bean parent="PropDev-SubmitPage-Keyword"  p:render="@{#ViewHelper.isSummaryKeywordsPanelEnabled()}" p:order="80" />
