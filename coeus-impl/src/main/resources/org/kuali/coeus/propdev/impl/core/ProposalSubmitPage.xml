                                  p:order="10"  />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="layoutManager.libraryCssClasses">
            <list>
                <value>table</value>
                <value>table-condensed</value>
            </list>
        </property>
        <property name="layoutManager.cssClasses">
            <null/>
        </property>
