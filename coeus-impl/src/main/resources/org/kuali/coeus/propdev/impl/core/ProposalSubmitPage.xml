                    <property name="control">
                        <bean parent="Uif-FileControl"/>
                    </property>
                </bean>
            </list>
        </property>

            </bean>

    <bean id="PropDev-SubmitPage-SummaryTabGroup" parent="PropDev-SubmitPage-SummaryTabGroup-parentBean" />
<bean id="PropDev-SubmitPage-SummaryTabGroup-parentBean" abstract="true" parent="Uif-TabGroup">
