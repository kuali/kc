    <bean id="PropDev-Resumbit-OptionsSection" parent="PropDev-Resumbit-OptionsSection-parentBean" />
	<bean id="PropDev-Resumbit-OptionsSection-parentBean" abstract="true" parent="Uif-DialogGroup" p:resetDataOnRefresh="true"
            p:dialogCssClass="modal-lg" p:header.headerText="Resubmission Options"   p:readOnly="false"
            p:onHideDialogScript="jQuery('#PropDev-Resumbit-OptionsSection').one(kradVariables.EVENTS.HIDDEN_MODAL, function(e){Kc.Dialog.resetDialogFields(jQuery('#PropDev-Resumbit-OptionsSection'));});">
		<property name="items">
			   <list>
				    <bean id="PropDev-Resumbit-OptionsRadio" parent="Uif-InputField" p:required="true"
				     p:propertyName="resubmissionOption"
				     p:order="10" >
				     <property name="control">
					      <bean parent="Uif-VerticalRadioControl">
					         <property name="options">
		                       <list>
			                        <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).GENERATE_NEW_VERSION_OF_IP}" p:value="Generate a new version of Institutional Proposal" />
	                                <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).GENERATE_NEW_IP}" p:value="Generate a new Institutional Proposal" />
	                                <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).DO_NOT_GENERATE_NEW_IP}" p:value="Do not generate a new Institutional Proposal or Institutional Proposal version" />
		                      </list>
				        	</property>
					     	<property name="propertyReplacers">
	            				<list>
	                				<bean parent="Uif-ConditionalBeanPropertyReplacer"
	                      				p:propertyName="options"
	                      				p:condition="@{!#empty(document.developmentProposal.continuedFrom)}">
					                    <property name="replacement">
					                    	<bean parent="Uif-VerticalRadioControl">
					                        	<property name="options">
						                            <list>
											            <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).GENERATE_NEW_VERSION_OF_ORIGINAL_IP}" p:value="Generate a new version of Original Institutional Proposal (@{document.developmentProposal.continuedFrom})" />
								                        <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).GENERATE_NEW_VERSION_OF_IP}" p:value="Generate a new version of Institutional Proposal" />
						                                <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).GENERATE_NEW_IP}" p:value="Generate a new Institutional Proposal" />
						                                <bean parent="Uif-KeyLabelPair" p:key="#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).DO_NOT_GENERATE_NEW_IP}" p:value="Do not generate a new Institutional Proposal or Institutional Proposal version" />
						                           </list>
					                            </property>
					                        </bean>
	                    				</property>
	                				</bean>
	            				</list>
        					</property>
   						</bean>
				     </property>
	    			</bean>
	    			<bean id="PropDev-Resumbit-Options-InstitutionalProposalToVersion"
					   parent="Uif-InputField" p:propertyName="institutionalProposalToVersion"
					  p:progressiveRender="@{resubmissionOption == '#{T(org.kuali.coeus.propdev.impl.core.ProposalDevelopmentConstants.ResubmissionOptions).GENERATE_NEW_VERSION_OF_IP}'}" p:fieldLabel.labelText="Institutional Proposal To Version">
					  	<property name="quickfinder">
