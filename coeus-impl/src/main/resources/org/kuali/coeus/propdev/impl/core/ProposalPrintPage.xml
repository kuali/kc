<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="PropDev-PrintSection" parent="PropDev-PrintSection-parentBean" />
	<bean id="PropDev-PrintSection-parentBean" abstract="true"
		parent="Uif-DialogBase">

		<property name="header">
			<bean parent="Uif-DialogHeader" p:headerText="Print document" />
		</property>

		<property name="items">
			<list>

				<bean parent="Uif-VerticalBoxGroup">
					<property name="items">
						<list>
							<bean parent="PropDev-PrintSection-Gov-Collection" p:order="5" />

						</list>
					</property>
				</bean>
				<bean parent="Uif-VerticalBoxGroup">
					<property name="items">
						<list>
							<bean parent="PropDev-PrintSection-Collection" p:order="10" />

						</list>
					</property>
				</bean>

				<bean parent="Uif-HorizontalBoxGroup">
					<property name="items">
						<list>
							<bean parent="PropDev-CurrentReport-SponsorField" p:order="20" />
							<bean parent="Uif-SecondaryActionButton" p:actionLabel="Generate report"
								p:methodToCall="printCurrentReport" p:order="30"
								p:successCallback="window.location.href='@{#ConfigProperties['application.url']}/kc-pd-krad/proposalDevelopment?methodToCall=printCurrentReport&amp;formKey=@{formKey}';" />
						</list>
					</property>
				</bean>

				<bean parent="Uif-HorizontalBoxGroup">
					<property name="items">
						<list>
							<bean parent="PropDev-PendingReport-SponsorField" p:order="40" />
							<bean parent="Uif-SecondaryActionButton" p:ajaxSubmit="false"
								p:actionLabel="Generate report" p:methodToCall="printPendingReport"
								p:order="50"
								p:successCallback="window.location.href='@{#ConfigProperties['application.url']}/kc-pd-krad/proposalDevelopment?methodToCall=printPendingReport&amp;formKey=@{formKey}';" />
						</list>
					</property>
				</bean>

			</list>
		</property>

		<property name="footer">
			<bean parent="Uif-DialogFooter">
				<property name="items">
					<list>
						<bean p:actionLabel="Print Selected" parent="Uif-PrimaryActionButton"
							p:methodToCall="printSelected"
							p:successCallback="window.location.href='@{#ConfigProperties['application.url']}/kc-pd-krad/proposalDevelopment?methodToCall=printSelected&amp;formKey=@{formKey}';" />
						<bean p:actionLabel="Cancel" parent="Uif-SecondaryActionButton"
							p:methodToCall="cancel" />

					</list>
				</property>
			</bean>
		</property>
	</bean>

	<bean id="PropDev-PrintSection-Gov-Collection" parent="PropDev-PrintSection-Gov-Collection-parentBean" />
	<bean id="PropDev-PrintSection-Gov-Collection-parentBean" parent="Uif-Disclosure-TableCollectionSection"
		p:headerText="Grants.gov packages" p:propertyName="s2sOppForms"
		p:collectionObjectClass="org.kuali.coeus.propdev.impl.s2s.S2sOppForms"
		p:renderAddLine="false" p:layoutManager.renderSequenceField="false"
		p:renderLineActions="false" p:pageSize="500"
		p:layoutManager.richTable.disableTableSort="true"
		p:layoutManager.richTable.showExportOption="false">
		<property name="items">
			<list>
				<bean parent="Uif-InputField" p:propertyName="selectToPrint"
					p:fieldLabel.labelText="[0]">
					<property name="control">
						<bean parent="Uif-CheckboxControl" />
					</property>
				</bean>
				<bean parent="Uif-DataField" p:propertyName="opportunityId" />
			</list>
		</property>
	</bean>
	<bean id="PropDev-PrintSection-Collection" parent="PropDev-PrintSection-Collection-parentBean" />
	<bean id="PropDev-PrintSection-Collection-parentBean" parent="Uif-Disclosure-TableCollectionSection"
		p:headerText="Sponsor form packages" p:propertyName="sponsorFormTemplates"
		p:collectionObjectClass="org.kuali.coeus.common.framework.sponsor.form.SponsorFormTemplateList"
		p:renderAddLine="false" p:layoutManager.renderSequenceField="false"
		p:renderLineActions="false" p:pageSize="500"
		p:layoutManager.richTable.disableTableSort="true"
		p:layoutManager.richTable.showExportOption="false">
		<property name="items">
			<list>
				<bean parent="Uif-InputField" p:propertyName="selectToPrint"
					p:fieldLabel.labelText="@{#line.getPageDescription()}">

					<property name="control">
						<bean parent="Uif-CheckboxControl" />
					</property>
				</bean>
				<bean parent="Uif-DataField" p:propertyName="pageDescription" />

			</list>
		</property>
	</bean>
	
		<bean id="PropDev-CurrentReport-SponsorField" parent="PropDev-CurrentReport-SponsorField-parentBean" />
	<bean id="PropDev-CurrentReport-SponsorField-parentBean" parent="Uif-InputField"
		p:propertyName="sponsor.sponsorCode">
		<property name="instructionalText"
			value="Click the button to perform a lookup on Sponsor" />
		<property name="quickfinder">
			<bean parent="Uif-QuickFinder" p:dataObjectClassName="org.kuali.coeus.common.framework.sponsor.Sponsor"
				p:fieldConversions="sponsorCode:sponsor.sponsorCode" />
		</property>
	</bean>

	<bean id="PropDev-PendingReport-SponsorField" parent="PropDev-PendingReport-SponsorField-parentBean" />
	<bean id="PropDev-PendingReport-SponsorField-parentBean" parent="Uif-InputField"
		p:propertyName="sponsor.sponsorCode">
		<property name="instructionalText"
			value="Click the button to perform a lookup on Sponsor" />
		<property name="quickfinder">
			<bean parent="Uif-QuickFinder" p:dataObjectClassName="org.kuali.coeus.common.framework.sponsor.Sponsor"
				p:fieldConversions="sponsorCode:sponsor.sponsorCode" />
		</property>
	</bean>
</beans>