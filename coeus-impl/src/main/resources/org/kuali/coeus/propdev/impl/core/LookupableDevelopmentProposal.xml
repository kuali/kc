<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="LookupableDevelopmentProposal" parent="LookupableDevelopmentProposal-parentBean" />
    <bean id="LookupableDevelopmentProposal-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.core.LookupableDevelopmentProposal" />
		<property name="objectLabel" value="Lookupable Development Proposal" />
        <property name="lookupDefinition" >
            <ref bean="LookupableDevelopmentProposal-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="LookupableDevelopmentProposalNumber" />

        <property name="attributes" >
            <list>
                <ref bean="LookupableDevelopmentProposal-proposalNumber" />
                <ref bean="LookupableDevelopmentProposal-title" />
				<ref bean="LookupableDevelopmentProposal-sponsorCode" />
                <ref bean="LookupableDevelopmentProposal-ownedByUnitNumber" />
				<ref bean="LookupableDevelopmentProposal-ownedByUnitName" />
				<ref bean="LookupableDevelopmentProposal-proposalTypeCode" />
				<ref bean="LookupableDevelopmentProposal-proposalTypeDescription" />
				<ref bean="LookupableDevelopmentProposal-deadlineDate" />
				<ref bean="LookupableDevelopmentProposal-investigator" />
				<ref bean="LookupableDevelopmentProposal-documentNumber" />
				<ref bean="LookupableDevelopmentProposal-sponsor.sponsorName" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="LookupableDevelopmentProposal-proposalNumber" parent="LookupableDevelopmentProposal-proposalNumber-parentBean" />
    <bean id="LookupableDevelopmentProposal-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Number" />
        <property name="shortLabel" value="Proposal Number" />
        <property name="maxLength" value="12" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="12"/>
    </property>
    <property name="controlField">
      <bean p:size="12" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal Number" />
        <property name="description" value="Proposal Number" />
    </bean>
    
    <bean id="LookupableDevelopmentProposal-documentNumber" parent="LookupableDevelopmentProposal-documentNumber-parentBean" />
    <bean id="LookupableDevelopmentProposal-documentNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="documentNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Document Number" />
        <property name="shortLabel" value="Document Number" />
        <property name="maxLength" value="40" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
    <property name="controlField">
      <bean p:size="40" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Document Number" />
        <property name="description" value="Document Number" />
    </bean>
    
    <bean id="LookupableDevelopmentProposal-title" parent="LookupableDevelopmentProposal-title-parentBean" />
    <bean id="LookupableDevelopmentProposal-title-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="title" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Title" />
        <property name="shortLabel" value="Proposal Title" />
        <property name="maxLength" value="150" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="30"/>
    </property>
    <property name="controlField">
      <bean p:size="30" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal Title" />
        <property name="description" value="Proposal Title" />
    </bean>
    
	<bean id="LookupableDevelopmentProposal-sponsorCode" parent="LookupableDevelopmentProposal-sponsorCode-parentBean" />
  <bean id="LookupableDevelopmentProposal-sponsorCode-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="sponsorCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Sponsor Code" />
		<property name="shortLabel" value="Sponsor" />
		<property name="maxLength" value="6" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint"/>
    </property>
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="12" />
		</property>
    <property name="controlField">
      <bean p:size="12" parent="Uif-TextControl"/>
    </property>
		<property name="summary" value="Agency/Sponsor #" />
    <property name="description" value="The identification number of the organization or agency that is providing support for the sponsored project."/>
	</bean>

	<bean id="LookupableDevelopmentProposal-ownedByUnitNumber" parent="LookupableDevelopmentProposal-ownedByUnitNumber-parentBean" />
	<bean id="LookupableDevelopmentProposal-ownedByUnitNumber-parentBean" abstract="true" parent="Unit-unitNumber">
		<property name="name" value="ownedByUnitNumber" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Lead Unit" />
		<property name="shortLabel" value="Lead Unit" />
		<property name="maxLength" value="8" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint"/>
    </property>
		<property name="required" value="true" />
		<property name="summary" value="Lead Unit (Owned by Unit)" />
    <property name="description" value="The proposal's fixed home unit that will be used for routing and business rule purposes. AKA Owned by Unit."/>
	</bean>
	
	<bean id="LookupableDevelopmentProposal-ownedByUnitName" parent="LookupableDevelopmentProposal-ownedByUnitName-parentBean" />
  <bean id="LookupableDevelopmentProposal-ownedByUnitName-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="ownedByUnitName" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Lead Unit Name" />
		<property name="shortLabel" value="Lead Unit Name" />
		<property name="maxLength" value="150" />
		<property name="validationPattern">
			<bean p:allowWhitespace="true" parent="AnyCharacterValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
    </property>
		<property name="control">
			<bean p:size="60" parent="TextControlDefinition" />
		</property>
    <property name="controlField">
      <bean p:size="60" parent="Uif-TextControl"/>
    </property>
		<property name="summary" value="Lead Unit Name (Owned by Unit)." />
    <property name="description" value="The proposal's fixed home unit that will be used for routing and business rule purposes. AKA Owned by Unit."/>
	</bean>

	<bean id="LookupableDevelopmentProposal-proposalTypeCode" parent="LookupableDevelopmentProposal-proposalTypeCode-parentBean" />
  <bean id="LookupableDevelopmentProposal-proposalTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="proposalTypeCode" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Proposal Type" />
		<property name="shortLabel" value="Proposal Type" />
		<property name="maxLength" value="3" />
		<property name="validationPattern">
			<bean parent="AlphaNumericValidationPattern" />
		</property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
		<property name="required" value="true" />
		<property name="control">
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.coeus.common.framework.type.ProposalType" p:valuesFinderClass="org.kuali.coeus.sys.framework.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:keyAttribute="code" p:labelAttribute="description"/>
    </property>
    <property name="optionsFinder">
      <bean class="org.kuali.coeus.sys.framework.keyvalue.ExtendedPersistableBusinessObjectValuesFinder">
        <property name="businessObjectClass" value="org.kuali.coeus.common.framework.type.ProposalType"/>
        <property name="includeKeyInDescription" value="false"/>
        <property name="keyAttributeName" value="code"/>
        <property name="labelAttributeName" value="description"/>
      </bean>
    </property>
    <property name="controlField">
      <bean parent="Uif-DropdownControl"/>
		</property>
		<property name="summary" value="The Types that the Proposal Could be." />
		<property name="description" value="The type of the proposal." />
	</bean>

	<bean id="LookupableDevelopmentProposal-proposalTypeDescription" parent="LookupableDevelopmentProposal-proposalTypeDescription-parentBean" />
  <bean id="LookupableDevelopmentProposal-proposalTypeDescription-parentBean" abstract="true" parent="ProposalType-description">
		<property name="name" value="proposalTypeDescription" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Proposal Type" />
		<property name="shortLabel" value="Proposal Type" />
		<property name="required" value="false" />
		<property name="summary" value="The Types that the Proposal Could be." />
		<property name="description" value="The type of the proposal." />
	</bean>

	<bean id="LookupableDevelopmentProposal-deadlineDate" parent="LookupableDevelopmentProposal-deadlineDate-parentBean" />
  <bean id="LookupableDevelopmentProposal-deadlineDate-parentBean" abstract="true" parent="AttributeReferenceDummy-genericDate">
		<property name="name" value="deadlineDate" />
		<property name="label" value="Sponsor Deadline Date" />
		<property name="shortLabel" value="Sponsor Deadline Date" />
		<property name="required" value="false" />
		<property name="summary" value="The Deadline Date of the Proposal" />
    <property name="description" value="The date in which the proposal is due to the sponsoring agency.  Additional deadlines may be set by the office authorized to submit proposals on behalf of the institution (e.g. sponsored programs, etc.)."/>
	</bean>
	
 	<bean id="LookupableDevelopmentProposal-investigator" parent="LookupableDevelopmentProposal-investigator-parentBean" />
	<bean id="LookupableDevelopmentProposal-investigator-parentBean" abstract="true" parent="AttributeDefinition" >
		<property name="name" value="investigator" />
		<property name="forceUppercase" value="false" />
		<property name="label" value="Investigator" />
		<property name="shortLabel" value="Investigator" />
		<property name="maxLength" value="60" />
		<property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
		</property>
		<property name="control" >
      <bean parent="TextControlDefinition" p:size="40"/>
    </property>
    <property name="controlField">
      <bean p:size="40" parent="Uif-TextControl"/>
		</property>
		<property name="summary" value="Investigator" />
		<property name="description" value="Investigator" />
	</bean>

    <bean id="LookupableDevelopmentProposal-sponsor.sponsorName" parent="LookupableDevelopmentProposal-sponsor.sponsorName-parentBean" />
    <bean id="LookupableDevelopmentProposal-sponsor.sponsorName-parentBean" abstract="true" parent="Sponsor-sponsorName">
        <property name="name" value="sponsor.sponsorName" />
        <property name="label" value="Sponsor Name" />
        <property name="shortLabel" value="Sponsor Name" />
        <property name="required" value="false" />    
    </bean>
    
    <bean id="LookupableDevelopmentProposal-lookupDefinition" parent="LookupableDevelopmentProposal-lookupDefinition-parentBean" />
  <bean id="LookupableDevelopmentProposal-LookupView" parent="LookupableDevelopmentProposal-LookupView-parentBean"/>
    <bean id="LookupableDevelopmentProposal-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
      <property name="title" value="Proposal Development Document Lookup" />
      <property name="lookupableID" value="lookupableDevelopmentProposalLookupable" /><property name="defaultSort" >
      <bean parent="SortDefinition"/>
      </property>
      <property name="lookupFields" >
        <list>
        <bean parent="FieldDefinition" p:attributeName="proposalNumber"/>
        <bean parent="FieldDefinition" p:attributeName="title"/>
        <bean parent="FieldDefinition" p:attributeName="investigator"/>
        <bean parent="FieldDefinition" p:attributeName="sponsorCode"/>
        <bean parent="FieldDefinition" p:attributeName="ownedByUnitNumber"/>
        <bean parent="FieldDefinition" p:attributeName="proposalTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="deadlineDate"/>
        </list>
      </property>
      <property name="resultFields" >
        <list>
        <bean parent="FieldDefinition" p:attributeName="proposalNumber" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="documentNumber"/>
        <bean parent="FieldDefinition" p:attributeName="title"/>
        <bean parent="FieldDefinition" p:attributeName="investigator"/>
        <bean parent="FieldDefinition" p:attributeName="sponsorCode"/>
        <bean parent="FieldDefinition" p:attributeName="ownedByUnitNumber"/>
        <bean parent="FieldDefinition" p:attributeName="proposalTypeDescription"/>
        <bean parent="FieldDefinition" p:attributeName="deadlineDate"/>
        </list>
      </property>
    </bean>

  <bean id="LookupableDevelopmentProposal-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
    <property name="dataObjectClassName" value="org.kuali.coeus.propdev.impl.core.LookupableDevelopmentProposal"/>
    <property name="headerText" value="Proposal Development Document Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="proposalNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="title" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="investigator" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="sponsorCode" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="ownedByUnitNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="proposalTypeCode" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="deadlineDate" parent="Uif-LookupCriteriaInputField"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean p:propertyName="proposalNumber" parent="Uif-DataField"/>
        <bean p:propertyName="documentNumber" parent="Uif-DataField"/>
        <bean p:propertyName="title" parent="Uif-DataField"/>
        <bean p:propertyName="investigator" parent="Uif-DataField"/>
        <bean p:propertyName="sponsorCode" parent="Uif-DataField"/>
        <bean p:propertyName="ownedByUnitNumber" parent="Uif-DataField"/>
        <bean p:propertyName="proposalTypeDescription" parent="Uif-DataField"/>
        <bean p:propertyName="deadlineDate" parent="Uif-DataField"/>
      </list>
    </property>
  </bean>

<bean id="AllDevelopmentProposals-LookupViewId" parent="AllDevelopmentProposals-lookupView-parentBean" />
   <bean id="AllDevelopmentProposals-lookupView-parentBean" abstract="true" parent="DevelopmentProposal-LookupView" p:viewName="AllLookupableDevelopmentProposals-LookupViewName" 
	p:headerText="All Proposal Development Document Lookup" >
	<property name="criteriaFields">
      <list >
        <bean p:propertyName="proposalNumber" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="proposalTypeCode" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="proposalStateTypeCode" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="title" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="proposalPerson" parent="Uif-LookupCriteriaInputField" p:fieldLabel.labelText="Proposal Person"/>
        <bean p:propertyName="principalInvestigatorName" parent="Uif-LookupCriteriaInputField"/>
       <bean p:propertyName="aggregator" parent="Uif-LookupCriteriaInputField" p:fieldLabel.labelText="Aggregator"  p:enableAutoDirectInquiry="false" p:defaultValue="@{#userSession == null ? '' : #userSession.getLoggedInUserPrincipalName()}">
            <property name="inquiry" >
                <bean parent="Uif-Inquiry" p:dataObjectClassName="org.kuali.rice.kim.impl.identity.PersonImpl"
                      p:inquiryParameters="aggregator:principalName" />
            </property>
        </bean>
        <bean p:propertyName="deadlineDate" parent="Uif-LookupCriteriaInputField"  p:ranged="true"/>
        <bean p:propertyName="sponsorCode" parent="Uif-LookupCriteriaInputField">
          <property name="inquiry">
              <bean parent="Uif-Inquiry" p:dataObjectClassName="org.kuali.coeus.common.framework.sponsor.Sponsor"
                    p:inquiryParameters="sponsorCode:sponsorCode"/>
          </property>
        </bean>
        <bean p:propertyName="sponsor.sponsorName" parent="Uif-LookupCriteriaInputField">
            <property name="inquiry">
                <bean parent="Uif-Inquiry" p:dataObjectClassName="org.kuali.coeus.common.framework.sponsor.Sponsor"
                      p:inquiryParameters="sponsorCode:sponsorCode"/>
            </property>
        </bean>
        <bean p:propertyName="ownedByUnitNumber" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField">
            <property name="inquiry">
                <bean parent="Uif-Inquiry" p:dataObjectClassName="org.kuali.coeus.common.framework.unit.Unit"
                      p:inquiryParameters="ownedByUnitNumber:unitNumber"/>
            </property>
        </bean>
        <bean p:propertyName="ownedByUnit.unitName" parent="Uif-LookupCriteriaInputField">
            <property name="inquiry">
                <bean parent="Uif-Inquiry" p:dataObjectClassName="org.kuali.coeus.common.framework.unit.Unit"
                      p:inquiryParameters="ownedByUnit.unitName:unitName"/>
            </property>
        </bean>
        <bean p:propertyName="currentAwardNumber" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="opportunityIdForGG" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="hierarchyStatus" parent="Uif-LookupCriteriaInputField"/>
      </list>
    </property>
  </bean>


<bean id="SearchDevelopmentProposals-LookupViewId" parent="SearchDevelopmentProposals-lookupView-parentBean" />
   <bean id="SearchDevelopmentProposals-lookupView-parentBean"  abstract="true" parent="LookupableDevelopmentProposal-LookupView" 
	p:viewName="SearchLookupableDevelopmentProposals-LookupViewName" p:headerText="Search Proposal Development Document Lookup"/>
	
<bean id="EnrouteDevelopmentProposals-LookupViewId" parent="EnrouteDevelopmentProposals-lookupView-parentBean"/>    
<bean id="EnrouteDevelopmentProposals-lookupView-parentBean" abstract="true" parent="DevelopmentProposal-LookupView"     p:viewName="EnrouteLookupableDevelopmentProposals-LookupViewName" p:headerText="Enroute Development Proposals Lookup" 
     p:viewHelperService="#{#getService('enrouteDevelopmentProposalLookupable')}">
     <property name="criteriaFields">
      <list>
        <bean p:propertyName="proposalNumber" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField" p:order="10"/>
        <bean p:propertyName="proposalTypeCode" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField" p:order="20"/>
        <bean p:propertyName="title" parent="Uif-LookupCriteriaInputField" p:order="30"/>
        <bean p:propertyName="deadlineDate" parent="Uif-LookupCriteriaInputField" p:order="40"/>
        <bean p:propertyName="sponsorCode" parent="Uif-LookupCriteriaInputField" p:order="50" />
        <bean p:propertyName="sponsor.sponsorName" parent="Uif-LookupCriteriaInputField" p:order="60" />
        <bean p:propertyName="ownedByUnitNumber" p:enableAutoQuickfinder="false" parent="Uif-LookupCriteriaInputField" p:order="70" />
        <bean p:propertyName="ownedByUnit.unitName" parent="Uif-LookupCriteriaInputField"  p:order="80"/>
        <bean p:propertyName="currentAwardNumber" parent="Uif-LookupCriteriaInputField" p:order="90" />
        <bean p:propertyName="opportunityIdForGG" parent="Uif-LookupCriteriaInputField" p:order="100"/>
        <bean p:propertyName="hierarchyStatus" parent="Uif-LookupCriteriaInputField" p:order="110"/>
        <bean p:propertyName="proposalDocument.documentNumber" parent="Uif-LookupCriteriaInputField" p:order="120"/>
      </list>
  </property>
       </bean>

</beans>
