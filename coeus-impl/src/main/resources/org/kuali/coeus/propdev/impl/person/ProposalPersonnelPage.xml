	<bean id="PropDev-PersonnelPage-ExtendedDetailsEducation" parent="PropDev-PersonnelPage-ExtendedDetailsEducation-parentBean" />
	<bean id="PropDev-PersonnelPage-ExtendedDetailsEducation-parentBean"
		abstract="true" parent="Uif-CssGridSubSection-2FieldLabelColumn"
		p:headerText="Education">
		<property name="items">
            <list>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="educationLevel"
                      p:order="10"/>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="yearGraduated"
                      p:order="20"/>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="major"
                      p:order="30"/>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="degree"
                      p:order="40"/>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="school"
