                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="postalCode"
                      p:order="160"/>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="division"
                      p:order="170"/>
                <bean parent="PropDev-PersonnelPage-InputField" p:propertyName="state"
                      p:order="180"
                      p:refreshWhenChangedPropertyNames="#lp.countryCode"/>
