				<bean parent="Uif-InputField" p:propertyName="pagerNumber"
					p:order="70" />
				<bean parent="Uif-InputField" p:propertyName="addressLine3"
					p:order="80" />					
				<bean parent="Uif-InputField" p:propertyName="mobilePhoneNumber"
