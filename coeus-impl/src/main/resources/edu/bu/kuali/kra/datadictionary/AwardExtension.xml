                <ref bean="AwardExtension-federalClinicalTrial"/>
                <!-- BUKC-0090: Adding new custom data element needed in KC: FAIN (ENHC0012305) -->
                <ref bean="AwardExtension-fain"/>
            </list>
        </property>
    </bean>
    <bean id="AwardExtension-awardId" parent="Award-awardId"/>

