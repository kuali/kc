<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.YnqExplanationType">

			<!--  
        <inquiry>
            <title>Ynq Explanation Type</title>

            <inquirySections><inquirySection title="Ynq Explanation Type"><inquiryFields>
            	<field attributeName="explanationType"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>
            -->

        <lookup>
            <title>Ynq Explanation Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Ynq Explanation Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="explanationType"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="explanationType" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="explanationType" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>explanationType</titleAttribute>
        <attributes>

	        <attribute name="explanationType">
			<label>Explanation Type </label>
			<shortLabel>Explanation Type </shortLabel>
			<maxLength>1</maxLength>
			<control>
				<hidden />
			</control>
			<summary>Explanation Type </summary>
			<description>Explanation Type </description>
			</attribute>

			<attribute name="description">
			<label>Description</label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<control>
				<hidden />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>
