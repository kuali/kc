<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.UnitAdministratorType">

        <inquiry>
            <title>Unit Administrator Type</title>

            <inquirySections><inquirySection title="Unit Administrator Type"><inquiryFields>
            	<field attributeName="unitAdministratorTypeCode" forceInquiry="true"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Unit Administrator Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Unit Administrator Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="unitAdministratorTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="unitAdministratorTypeCode" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="unitAdministratorTypeCode" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>unitAdministratorTypeCode</titleAttribute>
        <attributes>

	        <attribute name="unitAdministratorTypeCode">
			<label>Unit Administrator Type Code </label>
			<shortLabel>Unit Administrator Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Unit Administrator Type Code </summary>
			<description>Unit Administrator Type Code </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>30</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
			<control>
				<text size="30" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
