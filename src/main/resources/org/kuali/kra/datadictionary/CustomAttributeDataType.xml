<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.CustomAttributeDataType">

        <inquiry>
            <title>Custom Attribute Data Type</title>

            <inquirySections><inquirySection title="Custom Attribute Data Type"><inquiryFields>
            	<field attributeName="dataTypeCode" forceInquiry="true"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Custom Attribute Data Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Custom Attribute Data Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="dataTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="dataTypeCode" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="dataTypeCode" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>dataTypeCode</titleAttribute>
        <attributes>

	        <attribute name="dataTypeCode">
			<label>Data Type Code </label>
			<shortLabel>Data Type Code </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Data Type Code </summary>
			<description>Data Type Code </description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
