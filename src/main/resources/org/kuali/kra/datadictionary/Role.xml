<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.Role">

        <inquiry>
            <title>Role</title>

            <inquirySections><inquirySection title="Role"><inquiryFields>
            	<field attributeName="roleId" forceInquiry="true"/>
				<field attributeName="descendFlag"/>
				<field attributeName="description"/>
				<field attributeName="ownedByUnit"/>
				<field attributeName="roleName"/>
				<field attributeName="roleType"/>
				<field attributeName="statusFlag"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Role Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Role</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="roleId"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="roleId" required="false"/>
				<lookupField attributeName="descendFlag" required="false"/>
				<lookupField attributeName="description" required="false"/>
				<lookupField attributeName="ownedByUnit" required="false"/>
				<lookupField attributeName="roleName" required="false"/>
				<lookupField attributeName="roleType" required="false"/>
				<lookupField attributeName="statusFlag" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="roleId" forceInquiry="true"/>
				<field attributeName="descendFlag" forceInquiry="false"/>
				<field attributeName="description" forceInquiry="false"/>
				<field attributeName="ownedByUnit" forceInquiry="false"/>
				<field attributeName="roleName" forceInquiry="false"/>
				<field attributeName="roleType" forceInquiry="false"/>
				<field attributeName="statusFlag" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>roleId</titleAttribute>
        <attributes>

	        <attribute name="roleId">
			<label>Role Id </label>
			<shortLabel>Role Id </shortLabel>
			<maxLength>5</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="5" />
			</control>
			<summary>Role Id </summary>
			<description>Role Id </description>
			</attribute>

			<attribute name="descendFlag">
			<label>Descend Flag </label>
			<shortLabel>Descend Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Descend Flag </summary>
			<description>Descend Flag </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="200" />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

			<attribute name="ownedByUnit">
			<label>Owned By Unit </label>
			<shortLabel>Owned By Unit </shortLabel>
			<maxLength>8</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Owned By Unit </summary>
			<description>Owned By Unit </description>
			</attribute>

			<attribute name="roleName">
			<label>Role Name </label>
			<shortLabel>Role Name </shortLabel>
			<maxLength>50</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="50" />
			</control>
			<summary>Role Name </summary>
			<description>Role Name </description>
			</attribute>

			<attribute name="roleType">
			<label>Role Type </label>
			<shortLabel>Role Type </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Role Type </summary>
			<description>Role Type </description>
			</attribute>

			<attribute name="statusFlag">
			<label>Status Flag </label>
			<shortLabel>Status Flag </shortLabel>
			<maxLength>1</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="1" />
			</control>
			<summary>Status Flag </summary>
			<description>Status Flag </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
