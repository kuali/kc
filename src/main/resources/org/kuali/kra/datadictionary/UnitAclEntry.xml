<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.UnitAclEntry">

        <inquiry>
            <title>Person-Role</title>

            <inquirySections><inquirySection title="Person-Role"><inquiryFields>
                <field attributeName="personId"/>
                <field attributeName="roleId"/>
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Person-Role Lookup</title>
            <menubar>
                <![CDATA[<a href="index.jsp">Main</a>]]>
            </menubar>
            <instructions>Look up a Person-Role</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="personId"/>
                    <sortAttribute attributeName="roleId"/>
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="personId" required="false"/>
                <lookupField attributeName="roleId" required="false"/>
                <lookupField attributeName="unitNumber" required="false"/>
                <lookupField attributeName="subunits" required="false"/>
                <lookupField attributeName="active" required="false"/>
            </lookupFields>

            <resultFields>
                <field attributeName="personId" forceInquiry="false"/>
                <field attributeName="personName" forceInquiry="false"/>
                <field attributeName="roleId" forceInquiry="false"/>
                <field attributeName="roleName" forceInquiry="false"/>
                <field attributeName="roleTypeName" forceInquiry="false"/>
                <field attributeName="unitNumber" forceInquiry="false"/>
                <field attributeName="unitName" forceInquiry="false"/>
                <field attributeName="subunits" forceInquiry="false"/>
                <field attributeName="active" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>id</titleAttribute>
        <attributes>
            <attribute name="id">
                <label>Id </label>
                <shortLabel>Id </shortLabel>
                <maxLength>8</maxLength>
                <validationPattern><numeric /></validationPattern>
                <control>
                    <hidden />
                </control>
                <summary>Id </summary>
                <description>Id </description>
            </attribute>

			<attributeReference name="personId" sourceClassName="org.kuali.kra.bo.Person" sourceAttributeName="personId" />
            
             <attribute name="personName" >
                <label>Person Full Name</label>
                <shortLabel>Person Full Name</shortLabel>
                <maxLength>500</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>Person Full Name</summary>
                <description>Person Full Name</description>
            </attribute>

            <attribute name="roleId" >
                <label>Role Id</label>
                <shortLabel>Role Id</shortLabel>
                <maxLength>10</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>Role Id</summary>
                <description>Role Id</description>
            </attribute>
            
            <attribute name="roleName" >
                <label>Role Name</label>
                <shortLabel>Role Name</shortLabel>
                <maxLength>500</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>Role Name</summary>
                <description>Role Name</description>
            </attribute>
            
            <attribute name="roleTypeName" >
                <label>Role Type</label>
                <shortLabel>Role Type</shortLabel>
                <maxLength>500</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>Role Type</summary>
                <description>Role Type</description>
            </attribute>
            
            <attribute name="unitNumber" >
                <label>Unit Number</label>
                <shortLabel>Unit Number</shortLabel>
                <maxLength>10</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>Unit Number</summary>
                <description>Unit Number</description>
            </attribute>
            
            <attribute name="unitName" >
                <label>Unit Name</label>
                <shortLabel>Unit Name</shortLabel>
                <maxLength>500</maxLength>
                <control>
                    <text size="10" />
                </control>
                <summary>Unit Name</summary>
                <description>Unit Name</description>
            </attribute>
            
            <attributeReference name="subunits" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Descend</label>
                <shortLabel>Descend </shortLabel>
                <summary>Descend</summary>
                <description>Descend</description>
            </attributeReference>
           
            <attributeReference name="active" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericBoolean">
                <label>Active</label>
                <shortLabel>Active </shortLabel>
                <summary>Active</summary>
                <description>Active</description>
            </attributeReference>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>
    </businessObject>

</dictionaryEntry>
