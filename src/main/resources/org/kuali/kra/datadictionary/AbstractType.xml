            <resultFields>
                <field attributeName="abstractTypeCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>abstractTypeCode</titleAttribute>

        <attributes>

