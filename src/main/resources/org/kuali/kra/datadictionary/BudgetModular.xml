<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>

    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetModular">
    
		<titleAttribute>budgetModular</titleAttribute>
    
    	<attributes>
        
			<attribute name="proposalNumber">
				<label>Proposal Number </label>
				<shortLabel>Proposal Number </shortLabel>
				<maxLength>12</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>
				<control>
					<text size="8" />
				</control>
				<summary>Proposal Number </summary>
				<description>Proposal Number </description>
			</attribute>
			
			<attribute name="budgetVersionNumber">
				<label>Budget Version Number </label>
				<shortLabel>Budget Version Number </shortLabel>
				<maxLength>3</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<text size="3" />
				</control>
				<summary>Budget Version Number </summary>
				<description>Budget Version Number </description>
			</attribute>
			
			<attribute name="budgetPeriod">
				<label>Budget Period </label>
				<shortLabel>Budget Period </shortLabel>
				<maxLength>3</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<text size="3" />
				</control>
				<summary>Budget Period </summary>
				<description>Budget Period </description>
			</attribute>
			
			<attribute name="directCostLessConsortiumFna">
				<label>Direct Cost Less Consortium FNA </label>
				<shortLabel>Dir Cost Less FNA </shortLabel>
				<maxLength>12</maxLength>
				<validationPattern>
                    <fixedPoint allowNegative="false" precision="12" scale="2" />
                </validationPattern>
                <required>false</required>
                <control>
                    <currency size="16" formattedMaxLength="22" />
                </control>
				<summary>Direct Cost Less Consortium FNA</summary>
				<description>Direct Cost Less Consortium FNA</description>
			</attribute>
			
			<attribute name="consortiumFna">
				<label>Consortium FNA</label>
				<shortLabel>Consortium FNA</shortLabel>
				<maxLength>12</maxLength>
				<validationPattern>
                    <fixedPoint allowNegative="false" precision="12" scale="2" />
                </validationPattern>
                <required>false</required>
                <control>
                    <currency size="16" formattedMaxLength="22" />
                </control>
				<summary>Consortium FNA</summary>
				<description>Consortium FNA</description>
			</attribute>
			
			<attribute name="totalDirectCost">
				<label>Total Direct Cost</label>
				<shortLabel>TDC</shortLabel>
				<maxLength>12</maxLength>
				<validationPattern>
                    <fixedPoint allowNegative="false" precision="12" scale="2" />
                </validationPattern>
                <required>false</required>
                <control>
                    <currency size="16" formattedMaxLength="22" />
                </control>
				<summary>Total Direct Cost</summary>
				<description>Total Direct Cost</description>
			</attribute>
			
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
			
		</attributes>

        <objectHelp parameterNamespace="KRA-B" parameterDetailType="D" parameterName="budgetModularHelpUrl"/> 
    </businessObject>

</dictionaryEntry>
