<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.Unit">

        <inquiry>
            <title>Unit</title>

            <inquirySections><inquirySection title="Unit"><inquiryFields>
            	<field attributeName="unitNumber" forceInquiry="true"/>
				<field attributeName="administrativeOfficer"/>
				<field attributeName="deanVp"/>
				<field attributeName="organizationId"/>
				<field attributeName="ospAdministrator"/>
				<field attributeName="otherIndividualToNotify"/>
				<field attributeName="unitHead"/>
				<field attributeName="unitName"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Unit Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Unit</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="unitNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="unitNumber" required="false"/>
				<lookupField attributeName="administrativeOfficer" required="false"/>
				<lookupField attributeName="deanVp" required="false"/>
				<lookupField attributeName="organizationId" required="false"/>
				<lookupField attributeName="ospAdministrator" required="false"/>
				<lookupField attributeName="otherIndividualToNotify" required="false"/>
				<lookupField attributeName="unitHead" required="false"/>
				<lookupField attributeName="unitName" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="unitNumber" forceInquiry="true"/>
				<field attributeName="administrativeOfficer" forceInquiry="false"/>
				<field attributeName="deanVp" forceInquiry="false"/>
				<field attributeName="organizationId" forceInquiry="false"/>
				<field attributeName="ospAdministrator" forceInquiry="false"/>
				<field attributeName="otherIndividualToNotify" forceInquiry="false"/>
				<field attributeName="unitHead" forceInquiry="false"/>
				<field attributeName="unitName" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>unitNumber</titleAttribute>
        <attributes>

	        <attribute name="unitNumber">
			<label>Unit Number </label>
			<shortLabel>Unit Number </shortLabel>
			<maxLength>8</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Unit Number </summary>
			<description>Unit Number </description>
			</attribute>

			<attribute name="administrativeOfficer">
			<label>Administrative Officer </label>
			<shortLabel>Administrative Officer </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="9" />
			</control>
			<summary>Administrative Officer </summary>
			<description>Administrative Officer </description>
			</attribute>

			<attribute name="deanVp">
			<label>Dean Vp </label>
			<shortLabel>Dean Vp </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="9" />
			</control>
			<summary>Dean Vp </summary>
			<description>Dean Vp </description>
			</attribute>

			<attribute name="organizationId">
			<label>Organization Id </label>
			<shortLabel>Organization Id </shortLabel>
			<maxLength>8</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="8" />
			</control>
			<summary>Organization Id </summary>
			<description>Organization Id </description>
			</attribute>

			<attribute name="ospAdministrator">
			<label>Osp Administrator </label>
			<shortLabel>Osp Administrator </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="9" />
			</control>
			<summary>Osp Administrator </summary>
			<description>Osp Administrator </description>
			</attribute>

			<attribute name="otherIndividualToNotify">
			<label>Other Individual To Notify </label>
			<shortLabel>Other Individual To Notify </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="9" />
			</control>
			<summary>Other Individual To Notify </summary>
			<description>Other Individual To Notify </description>
			</attribute>

			<attribute name="unitHead">
			<label>Unit Head </label>
			<shortLabel>Unit Head </shortLabel>
			<maxLength>10</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="10" />
			</control>
			<summary>Unit Head </summary>
			<description>Unit Head </description>
			</attribute>

			<attribute name="unitName">
			<label>Unit Name </label>
			<shortLabel>Unit Name </shortLabel>
			<maxLength>60</maxLength>
			<validationPattern><alphaNumeric allowWhitespace="true"/></validationPattern>
			<control>
				<text size="60" />
			</control>
			<summary>Unit Name </summary>
			<description>Unit Name </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
