<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.SponsorForms">

        <inquiry>
            <title>Sponsor Forms</title>

            <inquirySections><inquirySection title="Sponsor Forms"><inquiryFields>
            	<field attributeName="packageName"/>
				<field attributeName="packageNumber"/>
				<field attributeName="sponsorCode"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Sponsor Forms Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Sponsor Forms</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="packageNumber" />
                	<sortAttribute attributeName="sponsorCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="packageName" required="false"/>
				<lookupField attributeName="packageNumber" required="false"/>
				<lookupField attributeName="sponsorCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="packageName" forceInquiry="false"/>
				<field attributeName="packageNumber" forceInquiry="false"/>
				<field attributeName="sponsorCode" forceInquiry="false"/>

            </resultFields>
        </lookup>

        
        <attributes>

	        <attribute name="packageName">
			<label>Package Name </label>
			<shortLabel>Package Name </shortLabel>
			<maxLength>200</maxLength>
			<control>
				<text size="50" />
			</control>
			<summary>Package Name </summary>
			<description>Package Name </description>
			</attribute>

			<attribute name="packageNumber">
			<label>Package Number </label>
			<shortLabel>Package Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Package Number </summary>
			<description>Package Number </description>
			</attribute>

			<attribute name="sponsorCode">
			<label>Sponsor Code </label>
			<shortLabel>Sponsor Code </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>Sponsor Code </summary>
			<description>Sponsor Code </description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>


    </businessObject>

</dictionaryEntry>
