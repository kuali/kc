<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>

    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetProjectIncome">
    
		<titleAttribute>budgetPeriod</titleAttribute>
        <attributes>
        	<!-- PK fields -->
			<attribute name="proposalNumber">
				<label>Proposal Number </label>
				<shortLabel>Proposal Number </shortLabel>
				<maxLength>12</maxLength>
				<validationPattern><alphaNumeric /></validationPattern>
				<control>
					<text size="8" />
				</control>
				<summary>Proposal Number </summary>
				<description>Proposal Number </description>
			</attribute>
			
			<attribute name="budgetVersionNumber">
				<label>Budget Version Number </label>
				<shortLabel>Budget Version Number </shortLabel>
				<maxLength>3</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<text size="3" />
				</control>
				<summary>Budget Version Number </summary>
				<description>Budget Version Number </description>
			</attribute>
			
			<attribute name="documentComponentId">
				<label>Document Component Id </label>
				<shortLabel>Document Component Id </shortLabel>
				<maxLength>3</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<text size="5" />
				</control>
				<summary>Document Component Id </summary>
				<description>Document Component Id </description>
			</attribute>
			
			<!-- Domain fields -->
			<attribute name="budgetPeriodNumber">
				<label>Budget Period </label>
				<shortLabel>Budget Period </shortLabel>
				<maxLength>3</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true"  />
				</validationPattern>
				<required>true</required>
				<control>
					<select
						valuesFinderClass="org.kuali.kra.budget.lookup.keyvalue.BudgetPeriodValuesFinder" />
				</control>
				<summary>Budget Period </summary>
				<description>Budget Period </description>
			</attribute>
			
			<attribute name="projectIncome">
				<label>Project Income </label>
				<shortLabel>Project Income </shortLabel>
				<maxLength>19</maxLength>
				<validationPattern>
                    <fixedPoint allowNegative="false" precision="19" scale="2" />
                </validationPattern>
                <required>true</required>
                <control>
                    <currency size="19" formattedMaxLength="27" />
                </control>
				<summary>Project Income </summary>
				<description>Project Income </description>
			</attribute>

			<attribute name="description">
				<label>Description </label>
				<shortLabel>Description  </shortLabel>
				<maxLength>200</maxLength>
				<validationPattern>
					<anyCharacter allowWhitespace="true"  />
				</validationPattern>
				<required>true</required>
				<control>
					<text size="60" />
				</control>
				<summary>Description  </summary>
				<description>Description  </description>
			</attribute>
		</attributes>

        <objectHelp parameterNamespace="KRA-B" parameterDetailType="D" parameterName="budgetProjectIncomeHelpUrl"/> 
    </businessObject>

</dictionaryEntry>
