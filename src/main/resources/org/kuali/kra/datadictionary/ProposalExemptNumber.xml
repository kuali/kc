<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProposalExemptNumber" parent="ProposalExemptNumber-parentBean" />

  <bean id="ProposalExemptNumber-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.proposaldevelopment.bo.ProposalExemptNumber" />
    <property name="inquiryDefinition" >
      <ref bean="ProposalExemptNumber-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="ProposalExemptNumber-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="exemptionTypeCode" />
    <property name="attributes" >
      <list>
        <ref bean="ProposalExemptNumber-proposalNumber" />
        <ref bean="ProposalExemptNumber-specialReviewNumber" />
        <ref bean="ProposalExemptNumber-exemptionTypeCode" />
        <ref bean="ProposalExemptNumber-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="ProposalExemptNumber-proposalNumber" parent="ProposalExemptNumber-proposalNumber-parentBean" />

  <bean id="ProposalExemptNumber-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Proposal Number" />
    <property name="shortLabel" value="Proposal Number" />
    <property name="maxLength" value="12" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Proposal Number" />
    <property name="description" value="Proposal Number" />
  </bean>
  <bean id="ProposalExemptNumber-specialReviewNumber" parent="ProposalExemptNumber-specialReviewNumber-parentBean" />

  <bean id="ProposalExemptNumber-specialReviewNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="specialReviewNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Special Review Number" />
    <property name="shortLabel" value="Special Review Number" />
    <property name="maxLength" value="3" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Special Review Number" />
    <property name="description" value="Special Review Number" />
  </bean>
  <bean id="ProposalExemptNumber-exemptionTypeCode" parent="ProposalExemptNumber-exemptionTypeCode-parentBean" />

  <bean id="ProposalExemptNumber-exemptionTypeCode-parentBean" abstract="true" parent="ExemptionType-exemptionTypeCode">
    <property name="control" >
      <bean parent="SelectControlDefinition"
            p:valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.ExemptionTypeValuesFinder"
            p:includeKeyInLabel="false" />
    </property>
  </bean>
  <bean id="ProposalExemptNumber-versionNumber" parent="ProposalExemptNumber-versionNumber-parentBean" />

  <bean id="ProposalExemptNumber-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="ProposalExemptNumber-inquiryDefinition" parent="ProposalExemptNumber-inquiryDefinition-parentBean" />

  <bean id="ProposalExemptNumber-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Prop Exempt Number" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="Prop Exempt Number" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="proposalNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="specialReviewNumber"
                    p:forceInquiry="true" />
              <bean parent="FieldDefinition"
                    p:attributeName="exemptionTypeCode" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="ProposalExemptNumber-lookupDefinition" parent="ProposalExemptNumber-lookupDefinition-parentBean" />

  <bean id="ProposalExemptNumber-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Prop Exempt Number Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up a Prop Exempt Number" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="specialReviewNumber" />
        <bean parent="FieldDefinition"
              p:attributeName="exemptionTypeCode" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="proposalNumber"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="specialReviewNumber"
              p:forceInquiry="true" />
        <bean parent="FieldDefinition"
              p:attributeName="exemptionTypeCode"
              p:forceInquiry="true" />
      </list>
    </property>
  </bean>
</beans>
