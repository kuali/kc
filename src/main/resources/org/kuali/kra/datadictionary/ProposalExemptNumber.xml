<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalExemptNumber" >

        <inquiry>
            <title>Prop Exempt Number</title>

            <inquirySections><inquirySection title="Prop Exempt Number"><inquiryFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="specialReviewNumber" forceInquiry="true"/>
				<field attributeName="exemptionTypeCode"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Prop Exempt Number Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Prop Exempt Number</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="specialReviewNumber"/>
					<sortAttribute attributeName="exemptionTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="specialReviewNumber" required="false"/>
				<lookupField attributeName="exemptionTypeCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="specialReviewNumber" forceInquiry="true"/>
				<field attributeName="exemptionTypeCode" forceInquiry="true"/>

            </resultFields>
        </lookup>

		<titleAttribute>exemptionTypeCode</titleAttribute>
        <attributes>

	        <attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="specialReviewNumber">
			<label>Special Review Number </label>
			<shortLabel>Special Review Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>Special Review Number </summary>
			<description>Special Review Number </description>
			</attribute>

			<attributeReference name="exemptionTypeCode" sourceClassName="org.kuali.kra.bo.ExemptionType" sourceAttributeName="exemptionTypeCode">
				<control>
					<select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.ExemptionTypeValuesFinder" />
				</control>
			</attributeReference>
			
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
