<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.SpecialReview">

        <inquiry>
            <title>Special Review</title>

            <inquirySections><inquirySection title="Special Review"><inquiryFields>
                <field attributeName="specialReviewCode" forceInquiry="true" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Special Review Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Special Review Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="specialReviewCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="specialReviewCode" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="specialReviewCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>specialReviewCode</titleAttribute>

        <attributes>

            <attribute name="specialReviewCode">
                <label>Special Review Code</label>
                <shortLabel>Special Review Code</shortLabel>

                <maxLength>3</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="4" />
                </control>

                <summary>Special Review Code</summary>
                <description>Special Review Code</description>
			</attribute>

            <attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>