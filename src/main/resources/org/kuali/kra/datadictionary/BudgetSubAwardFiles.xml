<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.budget.bo.BudgetSubAwardFiles">

        <inquiry>
            <title>Budget Sub Award Files</title>

            <inquirySections><inquirySection title="Budget sub Awards"><inquiryFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
            	<field attributeName="subAwardNumber" forceInquiry="true"/>
				<field attributeName="budgetVersionNumber" forceInquiry="true"/>				
				<field attributeName="versionNumber" forceInquiry="true"/>
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Budget Sub Awards</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Budget Sub Award</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>                	
					<sortAttribute attributeName="subAwardNumber"/>
					<sortAttribute attributeName="proposalNumber"/>
                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="subAwardNumber" required="false"/>
				<lookupField attributeName="budgetVersionNumber" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="versionNumber" required="false"/>
            </lookupFields>

            <resultFields>
            	<field attributeName="subAwardNumber" forceInquiry="true"/>
				<field attributeName="budgetVersionNumber" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="versionNumber" forceInquiry="true"/>
            </resultFields>
        </lookup>

        <titleAttribute>subAwardNumber</titleAttribute>
		
        <attributes>

	        <attribute name="subAwardNumber">
			<label>subAwardNumber</label>
			<shortLabel>subAwardNumber </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<hidden />
			</control>
			<summary>subAwardNumber </summary>
			<description>subAwardNumber </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>
			
			<attribute name="budgetVersionNumber">
			<label>Budget Version Number </label>
			<shortLabel>Budget Version Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Budget Version Number </summary>
			<description>Budget Version Number </description>
			</attribute>

			
			<attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
