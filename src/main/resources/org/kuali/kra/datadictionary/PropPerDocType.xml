<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.PropPerDocType">

        <inquiry>
            <title>Proposal Person Document Type Code</title>

            <inquirySections><inquirySection title="Proposal Person Document Type Code"><inquiryFields>
                <field attributeName="documentTypeCode" forceInquiry="true" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Science Code Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Person Document Type Code</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="documentTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="documentTypeCode" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="documentTypeCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>documentTypeCode</titleAttribute>

        <attributes>

            <attribute name="documentTypeCode">
                <label>Person Document Type Code</label>
                <shortLabel>Person Document Type Code</shortLabel>

                <maxLength>15</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>

                <control>
                    <text size="10" />
                </control>

                <summary>Person Document Type Code</summary>
                <description>Person Document Type Code</description>
			</attribute>

            <attribute name="description" forceUppercase="false">
                <label>Description</label>
                <shortLabel>Description</shortLabel>

                <maxLength>200</maxLength>
                <validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>

                <control>
                    <textarea rows="5" cols="40" />
                </control>

                <summary>Description</summary>
                <description>Description</description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>