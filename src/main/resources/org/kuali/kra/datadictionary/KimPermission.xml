<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="KimPermission" parent="KimPermission-parentBean" />

  <bean id="KimPermission-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.kim.bo.KimPermission" />
    <property name="inquiryDefinition" >
      <ref bean="KimPermission-inquiryDefinition" />
    </property>
    <property name="lookupDefinition" >
      <ref bean="KimPermission-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="id" />
    <property name="attributes" >
      <list>
        <ref bean="KimPermission-id" />
        <ref bean="KimPermission-name" />
        <ref bean="KimPermission-description" />
        <ref bean="KimPermission-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="KimPermission-id" parent="KimPermission-id-parentBean" />

  <bean id="KimPermission-id-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="id" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Permission Id" />
    <property name="shortLabel" value="Permission Id" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="NumericValidationPattern" />
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="summary" value="Permission Id" />
    <property name="description" value="Permission Id" />
  </bean>
  <bean id="KimPermission-name" parent="KimPermission-name-parentBean" />

  <bean id="KimPermission-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Permission Name" />
    <property name="shortLabel" value="Permission Name" />
    <property name="maxLength" value="500" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="40" />
    </property>
    <property name="summary" value="Permission Name" />
    <property name="description" value="Permission Name" />
  </bean>
  <bean id="KimPermission-description" parent="KimPermission-description-parentBean" />

  <bean id="KimPermission-description-parentBean" abstract="true" parent="KraAttributeReferenceDummy-bigDescription">
    <property name="name" value="description" />
    <property name="maxLength" value="4000" />
  </bean>
  <bean id="KimPermission-versionNumber" parent="KimPermission-versionNumber-parentBean" />

  <bean id="KimPermission-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber">
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="KimPermission-inquiryDefinition" parent="KimPermission-inquiryDefinition-parentBean" />

  <bean id="KimPermission-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="KIM Permission" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="KimPermission" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition"
                    p:attributeName="name" />
              <bean parent="FieldDefinition"
                    p:attributeName="description" />
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="KimPermission-lookupDefinition" parent="KimPermission-lookupDefinition-parentBean" />

  <bean id="KimPermission-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="KIM Permission Lookup" />
    <property name="menubar" value="&lt;a href=&quot;index.jsp&quot;&gt;Main&lt;/a&gt;" />
    <property name="instructions" value="Look up a KIM Permission" />
    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="name" />
        <bean parent="FieldDefinition"
              p:attributeName="description" />
      </list>
    </property>
  </bean>
</beans>
