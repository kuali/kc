<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalYnq">

        <inquiry>
            <title>Proposal Ynq</title>

            <inquirySections><inquirySection title="Proposal Ynq"><inquiryFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="questionId" forceInquiry="true"/>
				<field attributeName="answer"/>
				<field attributeName="explanation"/>
				<field attributeName="reviewDate"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Proposal Ynq Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Proposal Ynq</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="proposalNumber"/>
					<sortAttribute attributeName="questionId"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="questionId" required="false"/>
				<lookupField attributeName="answer" required="false"/>
				<lookupField attributeName="explanation" required="false"/>
				<lookupField attributeName="reviewDate" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="questionId" forceInquiry="true"/>
				<field attributeName="answer" forceInquiry="false"/>
				<field attributeName="explanation" forceInquiry="false"/>
				<field attributeName="reviewDate" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>proposalNumber</titleAttribute>
        <attributes>

	        <attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>8</maxLength>
			<control>
				<text size="8" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="questionId">
			<label>Question Id </label>
			<shortLabel>Question Id </shortLabel>
			<maxLength>4</maxLength>
            <validationPattern><anyCharacter/></validationPattern>
			<required>true</required>
			<control>
				<text size="4" />
			</control>
			<summary>Question Id </summary>
			<description>Question Id </description>
			</attribute>

			<attribute name="answer">
			<label>Answer </label>
			<shortLabel>Answer </shortLabel>
			<maxLength>1</maxLength>
            <validationPattern><anyCharacter exactLength="1" /></validationPattern>
			<control>
					<radio valuesFinderClass="org.kuali.kra.lookup.keyvalue.YnqAnswersValuesFinder" />
			</control>
			<summary>Answer </summary>
			<description>Answer </description>
			</attribute>

			<attribute name="dummyAnswer">
			<label>Answer </label>
			<shortLabel>Answer </shortLabel>
			<maxLength>1</maxLength>
			<control>
					<radio valuesFinderClass="org.kuali.kra.lookup.keyvalue.YnqDummyAnswersValuesFinder" />
			</control>
			<summary>Answer </summary>
			<description>Answer </description>
			</attribute>

			<attribute name="explanation">
			<label>Explanation </label>
			<shortLabel>Explanation </shortLabel>
			<maxLength>400</maxLength>
                <control>
                    <textarea rows="3" cols="20" />
                </control>
			<summary>Explanation </summary>
			<description>Explanation </description>
			</attribute>

			<attributeReference name="reviewDate" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate">
			<label>Review Date </label>
			<shortLabel>Review Date </shortLabel>
			<summary>Review Date </summary>
			<description>Review Date </description>
			</attributeReference>

		</attributes>
    </businessObject>

</dictionaryEntry>
