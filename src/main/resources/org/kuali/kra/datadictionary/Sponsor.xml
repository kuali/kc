<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.Sponsor">

        <inquiry>
            <title>Sponsor</title>

            <inquirySections><inquirySection title="Sponsor"><inquiryFields>
                <field attributeName="sponsorCode" forceInquiry="true"/>
                <field attributeName="acronym"/>
                <field attributeName="auditReportSentForFy"/>
                <field attributeName="cageNumber"/>
                <field attributeName="countryCode"/>
                <field attributeName="dodacNumber"/>
                <field attributeName="dunAndBradstreetNumber"/>
                <field attributeName="dunsPlusFourNumber"/>
                <field attributeName="unit.unitName"/>
                <field attributeName="postalCode"/>
                <field attributeName="rolodexId"/>
                <field attributeName="sponsorName"/>
                <field attributeName="sponsorType.description"/>
                <field attributeName="state"/>                

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
  		<lookupableID>sponsorLookupable</lookupableID> 
            <title>Sponsor Lookup</title>
            <menubar>
                <![CDATA[<a href="index.jsp">Main</a>]]>
            </menubar>
            <instructions>Look up a Sponsor</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="sponsorCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="sponsorCode" required="false"/>
                <lookupField attributeName="sponsorName" required="false"/>
                <lookupField attributeName="acronym" required="false"/>
                <lookupField attributeName="sponsorTypeCode" required="false" forceLookup="true"/>
                <lookupField attributeName="dunAndBradstreetNumber" required="false"/>
                <lookupField attributeName="dunsPlusFourNumber" required="false"/>
                <lookupField attributeName="dodacNumber" required="false"/>
                <lookupField attributeName="cageNumber" required="false"/>
                <lookupField attributeName="postalCode" required="false"/>
                <lookupField attributeName="state" required="false"/>
                <lookupField attributeName="countryCode" required="false" forceLookup="true" />
                <lookupField attributeName="auditReportSentForFy" required="false"/>
                <lookupField attributeName="ownedByUnit" required="false"/>
                <lookupField attributeName="rolodexId" required="false"/>                
            </lookupFields>

            <resultFields>
                <field attributeName="sponsorCode" forceInquiry="true"/>
                <field attributeName="sponsorName" forceInquiry="false"/>
                <field attributeName="acronym" forceInquiry="false"/>
                <field attributeName="sponsorType.description" forceInquiry="false"/>
                <field attributeName="dunAndBradstreetNumber" forceInquiry="false"/>
                <field attributeName="dodacNumber" forceInquiry="false"/>
                <field attributeName="cageNumber" forceInquiry="false"/>
                <field attributeName="state" forceInquiry="false"/>
                <field attributeName="countryCode" forceInquiry="false"/>
            </resultFields>
            <resultSetLimit>50</resultSetLimit>
        </lookup>

        <titleAttribute>sponsorCode</titleAttribute>
        <attributes>

            <attribute name="sponsorCode">
            <label>Sponsor Code </label>
            <shortLabel>Sponsor Code </shortLabel>
            <maxLength>6</maxLength>
            <!--
            // performing validation in org.kuali.kra.rules.SponsorMaintenanceDocumentRule
            // to provide more informative error messages
            <validationPattern><alphaNumeric allowUnderscore="false" allowWhitespace="false" exactLength="6"/></validationPattern>
            -->
            <control>
                <text size="6" />
            </control>
            <summary>Sponsor Code </summary>
            <description>Sponsor Code </description>
            </attribute>

            <attribute name="acronym">
            <label>Acronym </label>
            <shortLabel>Acronym </shortLabel>
            <maxLength>10</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="10" />
            </control>
            <summary>Acronym </summary>
            <description>Acronym </description>
            </attribute>

            <attribute name="auditReportSentForFy">
            <label>Audit Report Sent For Fy </label>
            <shortLabel>Audit Report Sent For Fy </shortLabel>
            <maxLength>4</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="4" />
            </control>
            <summary>Audit Report Sent For Fy </summary>
            <description>Audit Report Sent For Fy </description>
            </attribute>

            <attribute name="cageNumber">
            <label>CAGE Number </label>
            <shortLabel>CAGE Number </shortLabel>
            <maxLength>20</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="20" />
            </control>
            <summary>CAGE Number </summary>
            <description>CAGE Number </description>
            </attribute>

            <attribute name="dodacNumber">
            <label>DODAC Number </label>
            <shortLabel>DODAC Number </shortLabel>
            <maxLength>20</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="20" />
            </control>
            <summary>DODAC Number </summary>
            <description>DODAC Number </description>
            </attribute>

            <attribute name="dunAndBradstreetNumber">
            <label>DUN And Bradstreet Number </label>
            <shortLabel>DUN And Bradstreet Number </shortLabel>
            <maxLength>20</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="20" />
            </control>
            <summary>DUN And Bradstreet Number </summary>
            <description>DUN And Bradstreet Number </description>
            </attribute>

            <attribute name="dunsPlusFourNumber">
            <label>DUNS Plus Four Number </label>
            <shortLabel>DUNS Plus Four Number </shortLabel>
            <maxLength>20</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="20" />
            </control>
            <summary>DUNS Plus Four Number </summary>
            <description>DUNS Plus Four Number </description>
            </attribute>

            <attribute name="ownedByUnit">
            <label>Owned By Unit </label>
            <shortLabel>Owned By Unit </shortLabel>
            <maxLength>8</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="8" />
            </control>
            <summary>Owned By Unit </summary>
            <description>Owned By Unit </description>
            </attribute>

            <attributeReference name="unit.unitName" sourceClassName="org.kuali.kra.bo.Unit" sourceAttributeName="unitName" >
                <label>Owned By Unit </label>
                <shortLabel>Owned By Unit </shortLabel>
            </attributeReference>

            <attribute name="postalCode">
            <label>Postal Code </label>
            <shortLabel>Postal Code </shortLabel>
            <maxLength>15</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="15" />
            </control>
            <summary>Postal Code </summary>
            <description>Postal Code </description>
            </attribute>

            <attribute name="rolodexId">
            <label>Rolodex Id </label>
            <shortLabel>Rolodex Id </shortLabel>
            <maxLength>6</maxLength>
            <validationPattern><numeric /></validationPattern>
            <control>
                <text size="6" />
            </control>
            <summary>Rolodex Id </summary>
            <description>Rolodex Id </description>
            </attribute>

            <attribute name="sponsorName">
            <label>Sponsor Name </label>
            <shortLabel>Sponsor Name </shortLabel>
            <maxLength>60</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>
                <text size="60" />
            </control>
            <summary>Sponsor Name </summary>
            <description>The name of the sponsoring agency.</description>
            </attribute>

            <attribute name="sponsorTypeCode">
            <label>Sponsor Type Code </label>
            <shortLabel>Sponsor Type Code </shortLabel>
            <maxLength>3</maxLength>
            <validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
            <control>                
                <select valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.bo.SponsorType" keyAttribute="sponsorTypeCode" labelAttribute="description" includeKeyInLabel="false"/>
            </control>
            <summary>Sponsor Type Code </summary>
            <description>Sponsor Type Code </description>
            </attribute>

            <attributeReference name="sponsorType.description" sourceClassName="org.kuali.kra.bo.SponsorType" sourceAttributeName="description" >
                <label>Sponsor Type</label>
                <shortLabel>Sponsor Type</shortLabel>
            </attributeReference>

            <attributeReference name="state" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="state" />

            <attributeReference name="countryCode" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="countryCode" />

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
        </attributes>

        <relationships>
            <relationship objectAttributeName="unit">
                <primitiveAttribute sourceName="ownedByUnit" targetName="unitNumber" />
                <supportAttribute sourceName="unit.unitName" targetName="unitName" identifier="true" />
            </relationship>

            <relationship objectAttributeName="sponsorType">
                <primitiveAttribute sourceName="sponsorTypeCode" targetName="sponsorTypeCode" />
                <supportAttribute sourceName="sponsorType.description" targetName="description" identifier="true" />
            </relationship>
            
            <relationship objectAttributeName="rolodex">
                <primitiveAttribute sourceName="rolodexId" targetName="rolodexId" />
                <supportAttribute sourceName="rolodex.rolodexId" targetName="rolodexId" identifier="true" />
            </relationship>
        </relationships>

        <objectHelp parameterNamespace="KRA-PD" parameterDetailType="D" parameterName="proposalDevelopmentSponsorHelpUrl"/> 
    </businessObject>

</dictionaryEntry>
