<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ExpeditedReviewCheckListMaintenanceDocument" parent="ExpeditedReviewCheckListMaintenanceDocument-parentBean" />

  <bean id="ExpeditedReviewCheckListMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.irb.bo.ExpeditedReviewCheckListItem" />
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ExpeditedReviewCheckListMaintenanceDocument-EditExpeditedReviewCheckLists" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>expeditedReviewCheckListCode</value>
      </list>
    </property>
    <property name="label" value="Expedited Review CheckList Maintenance Document" />
    <property name="shortLabel" value="expeditedReviewCheckListMaintDoc" />
    <property name="summary" value="Expedited Review CheckList Maintenance Document" />
    <property name="description" value="Document used to create or update Expedited Review CheckList Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-M"
            p:parameterName="expeditedReviewCheckListMaintenanceHelp"
            p:parameterDetailType="D" />
    </property>
    <property name="documentTypeName" value="ExpeditedReviewCheckListMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ERCK" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ExpeditedReviewCheckListMaintenanceDocument-EditExpeditedReviewCheckLists" parent="ExpeditedReviewCheckListMaintenanceDocument-EditExpeditedReviewCheckLists-parentBean" />

  <bean id="ExpeditedReviewCheckListMaintenanceDocument-EditExpeditedReviewCheckLists-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Expedited Review CheckLists" />
    <property name="title" value="Edit Expedited Review CheckLists" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="expeditedReviewCheckListCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
