<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetCategoryMapMaintenanceDocument" parent="BudgetCategoryMapMaintenanceDocument-parentBean" />

  <bean id="BudgetCategoryMapMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.budget.bo.BudgetCategoryMap" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="BudgetCategoryMapMaintenanceDocument-EditBudgetCategoryMaps" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>mappingName</value>
        <value>targetCategoryCode</value>
      </list>
    </property>
    <property name="label" value="Budget Category Maps Maintenance Document" />
    <property name="shortLabel" value="budgetCategoryMapsMaintDoc" />
    <property name="summary" value="BudgetCategoryMaps Maintenance Document" />
    <property name="description" value="Document used to create or update BudgetCategoryMaps Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-M"
            p:parameterName="budgetCategoryMapMaintenanceHelp"
            p:parameterDetailType="D" />
    </property>
    <property name="documentTypeName" value="BudgetCategoryMapMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="BUCM" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="BudgetCategoryMapMaintenanceDocument-EditBudgetCategoryMaps" parent="BudgetCategoryMapMaintenanceDocument-EditBudgetCategoryMaps-parentBean" />

  <bean id="BudgetCategoryMapMaintenanceDocument-EditBudgetCategoryMaps-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Budget Category Maps" />
    <property name="title" value="Edit Budget Category Maps" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="mappingName"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="targetCategoryCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="categoryType" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
