<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "../../../../core/datadictionary/dataDictionary-1_0.dtd">

<dictionaryEntry>
    <maintenanceDocument>
        <businessObjectClass>org.kuali.kra.proposaldevelopment.bo.SpecialReview</businessObjectClass>
        <maintainableClass>org.kuali.core.maintenance.KualiMaintainableImpl</maintainableClass>

        <documentAuthorizerClass>org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase</documentAuthorizerClass>
        <authorizations>
            <authorization action="initiate">
                <workgroups>
                    <workgroup>WorkflowAdmin</workgroup>
                </workgroups>
            </authorization>
        </authorizations>

        <documentTypeName>SpecialReviewMaintenanceDocument</documentTypeName>
        <documentTypeCode>SPRV</documentTypeCode>

        <label>Special Review Maintenance Document</label>
        <shortLabel>specialReviewMaintDoc</shortLabel>

        <summary>Special Review Maintenance Document</summary>
        <description>Document used to create or update Special Review Reference objects</description>

        <lockingKeys>
            <field attributeName="specialReviewCode" />
        </lockingKeys>

        <maintainableSections>
            <maintainableSection title="Edit Special Review Codes">
                <maintainableItems>
                    <maintainableField name="specialReviewCode" required="true" />
                    <maintainableField name="description" required="true" />
                    <maintainableField name="versionNumber" />
                </maintainableItems>
            </maintainableSection>
        </maintainableSections>
    </maintenanceDocument>
</dictionaryEntry>
