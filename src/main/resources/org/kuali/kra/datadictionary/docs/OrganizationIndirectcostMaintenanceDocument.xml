<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="OrganizationIndirectcostMaintenanceDocument" parent="OrganizationIndirectcostMaintenanceDocument-parentBean" />

  <bean id="OrganizationIndirectcostMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.bo.OrganizationIndirectcost" />
    <property name="maintainableClass" value="org.kuali.core.maintenance.KualiMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="OrganizationIndirectcostMaintenanceDocument-EditOrganizationIndirectcost" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>idcNumber</value>
        <value>organizationId</value>
      </list>
    </property>
    <property name="label" value="OrganizationIndirectcost Maintenance Document" />
    <property name="shortLabel" value="organizationIndirectcostMaintDoc" />
    <property name="summary" value="OrganizationIndirectcost Maintenance Document" />
    <property name="description" value="Document used to create or update OrganizationIndirectcost Reference objects" />
    <property name="documentTypeName" value="OrganizationIndirectcostMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="ORIN" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="OrganizationIndirectcostMaintenanceDocument-EditOrganizationIndirectcost" parent="OrganizationIndirectcostMaintenanceDocument-EditOrganizationIndirectcost-parentBean" />

  <bean id="OrganizationIndirectcostMaintenanceDocument-EditOrganizationIndirectcost-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit OrganizationIndirectcost" />
    <property name="title" value="Edit OrganizationIndirectcost" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="idcNumber"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="organizationId"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="applicableIndirectcostRate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="endDate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="idcComment" />
        <bean parent="MaintainableFieldDefinition"
              p:name="idcRateTypeCode" />
        <bean parent="MaintainableFieldDefinition"
              p:name="requestedDate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="startDate" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
