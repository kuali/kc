<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PostSubmissionStatusMaintenanceDocument" parent="PostSubmissionStatusMaintenanceDocument-parentBean"/>

  <bean id="PostSubmissionStatusMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.proposaldevelopment.bo.PostSubmissionStatus"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="PostSubmissionStatusMaintenanceDocument-EditPostSubmissionStatusTypes"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>postSubmissionStatusCode</value>
      </list>
    </property>
    
    
    
    
    
    <property name="documentTypeName" value="PostSubmissionStatusMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    
    
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PostSubmissionStatusMaintenanceDocument-EditPostSubmissionStatusTypes" parent="PostSubmissionStatusMaintenanceDocument-EditPostSubmissionStatusTypes-parentBean"/>

  <bean id="PostSubmissionStatusMaintenanceDocument-EditPostSubmissionStatusTypes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Post Submission Status Types"/>
    <property name="title" value="Edit Post Submission Status Types"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="postSubmissionStatusCode" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="definition" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
      </list>
    </property>
  </bean>
</beans>