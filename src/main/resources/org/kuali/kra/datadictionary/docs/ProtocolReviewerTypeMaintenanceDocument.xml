<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="ProtocolReviewerTypeMaintenanceDocument" parent="ProtocolReviewerTypeMaintenanceDocument-parentBean" />

  <bean id="ProtocolReviewerTypeMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.kra.irb.bo.ProtocolReviewerType" />
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
    <property name="maintainableSections" >
      <list>
        <ref bean="ProtocolReviewerTypeMaintenanceDocument-EditProtocolReviewerTypes" />
      </list>
    </property>
    <property name="lockingKeys" >
      <list>
        <value>reviewerTypeCode</value>
      </list>
    </property>
    <property name="label" value="Protocol Reviewer Type Maintenance Document" />
    <property name="shortLabel" value="reviewerTypeMaintDoc" />
    <property name="summary" value="Expedited Review CheckList Maintenance Document" />
    <property name="description" value="Document used to create or update Protocol Review Type Reference objects" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KRA-M"
            p:parameterName="reviewerTypeCodeMaintenanceHelp"
            p:parameterDetailType="D" />
    </property>
    <property name="documentTypeName" value="ProtocolReviewerTypeMaintenanceDocument" />
    <property name="documentAuthorizerClass" value="org.kuali.core.document.authorization.MaintenanceDocumentAuthorizerBase" />
    <property name="documentTypeCode" value="PRRT" />
    <property name="authorizations" >
      <list>
        <bean parent="AuthorizationDefinition">
          <property name="action" value="initiate" />
          <property name="authorizedGroups" >
            <set>
              <value>WorkflowAdmin</value>
            </set>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="ProtocolReviewerTypeMaintenanceDocument-EditProtocolReviewerTypes" parent="ProtocolReviewerTypeMaintenanceDocument-EditProtocolReviewerTypes-parentBean" />
  <bean id="ProtocolReviewerTypeMaintenanceDocument-EditProtocolReviewerTypes-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Reviewer Types" />
    <property name="title" value="Edit Reviewer Types" />
    <property name="maintainableItems" >
      <list>
        <bean parent="MaintainableFieldDefinition"
              p:name="reviewerTypeCode"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="description"
              p:required="true" />
        <bean parent="MaintainableFieldDefinition"
              p:name="versionNumber" />
      </list>
    </property>
  </bean>
</beans>
