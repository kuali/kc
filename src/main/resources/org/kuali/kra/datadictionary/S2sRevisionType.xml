<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.S2sRevisionType">

        <inquiry>
            <title>S2s Revision Type</title>

            <inquirySections><inquirySection title="S2s Revision Type"><inquiryFields>
            	<field attributeName="s2sRevisionTypeCode" forceInquiry="true"/>
				<field attributeName="description"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>S2s Revision Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a S2s Revision Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="s2sRevisionTypeCode"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="s2sRevisionTypeCode" required="false"/>
				<lookupField attributeName="description" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="s2sRevisionTypeCode" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>

            </resultFields>
        </lookup>

        <titleAttribute>s2sRevisionTypeCode</titleAttribute>
        <attributes>

	        <attribute name="s2sRevisionTypeCode">
			<label>Revision</label>
			<shortLabel>Revision</shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
                <control>                    
                    <radio valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.bo.S2sRevisionType" keyAttribute="s2sRevisionTypeCode" labelAttribute="description" includeKeyInLabel="false"/>                   
                </control>
			<summary>S2s Revision Type Code </summary>
			<description>S2s Revision Type Code </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
                <control>
                    <textarea rows="5" cols="40" />
                </control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />

		</attributes>
    </businessObject>

</dictionaryEntry>
