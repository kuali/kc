<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.s2s.bo.S2sSubmissionType">    

        <inquiry>
            <title>Submission Type </title>

            <inquirySections><inquirySection title="Submission Type "><inquiryFields>
                <field attributeName="s2sSubmissionTypeCode" forceInquiry="true" />
                <field attributeName="description" />
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Submission Type Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Submission Type</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                    <sortAttribute attributeName="s2sSubmissionTypeCode" />
                </sortAttributes>
            </defaultSort>

            <lookupFields>
                <lookupField attributeName="s2sSubmissionTypeCode" required="false" />
                <lookupField attributeName="description" required="false" />
            </lookupFields>

            <resultFields>
                <field attributeName="s2sSubmissionTypeCode" forceInquiry="true" />
                <field attributeName="description" forceInquiry="false"/>
            </resultFields>
        </lookup>

        <titleAttribute>s2sSubmissionTypeCode</titleAttribute>

        <attributes>
            <attribute name="s2sSubmissionTypeCode">
                <label>Submission Type</label>
                <shortLabel>Submission Type</shortLabel>
                <maxLength>15</maxLength>
                <validationPattern><alphaNumeric /></validationPattern>
                <required>false</required>                
                <control> 
					<select valuesFinderClass="org.kuali.core.lookup.keyvalues.PersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.s2s.bo.S2sSubmissionType" keyAttribute="s2sSubmissionTypeCode" labelAttribute="description" includeKeyInLabel="false"/>
					<!-- <text size="5"/> -->															
				</control>
                <summary>Submission Type</summary>
                <description>Submission Type</description>
			</attribute>
						
			<attributeReference name="description" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="description" />
            <attributeReference name="versionNumber" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="versionNumber" />
		</attributes>
    </businessObject>

</dictionaryEntry>