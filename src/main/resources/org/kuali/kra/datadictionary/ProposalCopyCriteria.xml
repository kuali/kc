<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalCopyCriteria">

        <attributes>

			<attribute name="includeAttachments">
				<label>Attachments?</label>
				<shortLabel>Attachments?</shortLabel>
				<maxLength>1</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<control>
					<checkbox />
				</control>
				<summary>Include Attachments?</summary>
				<description>Include Attachments in the Proposal Copy?</description>
			</attribute>
			
			<attribute name="includeBudget">
				<label>Budget?</label>
				<shortLabel>Budget?</shortLabel>
				<maxLength>1</maxLength>
				<validationPattern><anyCharacter /></validationPattern>
				<control>
					<checkbox />
				</control>
				<summary>Include Budget?</summary>
				<description>Include Budget(s) in the Proposal Copy?</description>
			</attribute>
			
	        <attribute name="budgetVersions">
				<label>Budget Versions</label>
				<shortLabel>Budget Versions</shortLabel>
				<maxLength>1</maxLength>
				<validationPattern><numeric /></validationPattern>
				<required>false</required>
				<control>
					<select valuesFinderClass="org.kuali.kra.proposaldevelopment.lookup.keyvalue.CopyBudgetVersionsValuesFinder" />
				</control>
				<summary>Budget Versions</summary>
				<description>Budget Versions</description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
