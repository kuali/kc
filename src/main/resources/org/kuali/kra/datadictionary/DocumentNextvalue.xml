<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.bo.DocumentNextvalue">

        <inquiry>
            <title>Proposal Nextvalue</title>

            <inquirySections><inquirySection title="Proposal Nextvalue"><inquiryFields>
            	<field attributeName="propertyName" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="nextValue"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Proposal Nextvalue Lookup</title>
			<menubar>
				<![CDATA[<a href="/kra-dev/">Main</a>]]>
			</menubar>
            <instructions>Look up a Proposal Nextvalue</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="propertyName"/>
					<sortAttribute attributeName="proposalNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="propertyName" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="nextValue" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="propertyName" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="nextValue" forceInquiry="false"/>

            </resultFields>
        </lookup>

		<titleAttribute>proposalNumber</titleAttribute>
        <attributes>

	        <attribute name="propertyName">
			<label>Property Name </label>
			<shortLabel>Property Name </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter /></validationPattern>
			<control>
				<text size="200" />
			</control>
			<summary>Property Name </summary>
			<description>Property Name </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Number </summary>
			<description>Proposal Number </description>
			</attribute>

			<attribute name="nextValue">
			<label>Next Value </label>
			<shortLabel>Next Value </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="12" />
			</control>
			<summary>Next Value </summary>
			<description>Next Value </description>
			</attribute>

		</attributes>
    </businessObject>

</dictionaryEntry>
