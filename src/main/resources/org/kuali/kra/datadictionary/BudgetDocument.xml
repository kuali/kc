<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="BudgetDocument" parent="BudgetDocument-parentBean"/>

  <bean id="BudgetDocument-parentBean" abstract="true" parent="TransactionalDocumentEntry">
    
    <property name="usePessimisticLocking" value="true"/>
    <property name="useWorkflowPessimisticLocking" value="false"/>
    
    <property name="documentTypeName" value="BudgetDocument"/>
    <property name="documentClass" value="org.kuali.kra.budget.document.BudgetDocument"/>
    <property name="businessRulesClass" value="org.kuali.kra.budget.document.BudgetDocumentRule"/>
    <property name="documentAuthorizerClass" value="org.kuali.kra.budget.document.authorization.BudgetDocumentAuthorizer"/>
    
    <property name="allowsCopy" value="true"/>
    <property name="headerNavigationList">
      <list>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetVersionsHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Budget Versions"/>
          <property name="headerTabNavigateTo" value="versions"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetParametersHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Parameters"/>
          <property name="headerTabNavigateTo" value="parameters"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetRatesHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Rates"/>
          <property name="headerTabNavigateTo" value="rates"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetSummaryHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Summary"/>
          <property name="headerTabNavigateTo" value="summaryTotals"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetPersonnelHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Personnel"/>
          <property name="headerTabNavigateTo" value="personnel"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetNonPersonnelHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Non-Personnel"/>
          <property name="headerTabNavigateTo" value="expenses"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetDistributionAndIncomeHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Distribution &amp; Income"/>
          <property name="headerTabNavigateTo" value="distributionAndIncome"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetModularBudgetHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Modular Budget"/>
          <property name="headerTabNavigateTo" value="modularBudget"/>
        </bean>
        <bean parent="HeaderNavigation">
          <property name="helpDefinition">
            <bean parent="HelpDefinition" p:parameterNamespace="KRA-B" p:parameterName="budgetActionsHelp" p:parameterDetailType="D"/>
          </property>
          <property name="headerTabDisplayName" value="Budget Actions"/>
          <property name="headerTabNavigateTo" value="budgetActions"/>
        </bean>
      </list>
    </property>
    
    <property name="attributes">
      <list>
        <ref bean="BudgetDocument-versionNumber"/>
      </list>
    </property>
    
    <property name="workflowAttributes">
    	<ref bean="BudgetDocument-workflowAttributes"/>
    </property>
    
  </bean>

  <bean id="BudgetDocument-versionNumber" parent="BudgetDocument-versionNumber-parentBean"/>
  <bean id="BudgetDocument-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>

  <bean id="BudgetDocument-workflowAttributes" parent="BudgetDocument-workflowAttributes-parentBean"/>
  <bean id="BudgetDocument-workflowAttributes-parentBean" abstract="true" parent="WorkflowAttributes"/>
</beans>