<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.s2s.bo.S2sApplication">
    
    <inquiry>
            <title>S2s Application</title>

            <inquirySections><inquirySection title="S2s Application"><inquiryFields>
            	<field attributeName="proposalNumber" forceInquiry="true"/>
            	<field attributeName="application" />            	            	
            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>S2s Application</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up an S2s Application</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="application"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>            	
				<lookupField attributeName="proposalNumber"  required="false"/>
            	<lookupField attributeName="application"  required="false"/>            	            	
            </lookupFields>

            <resultFields>
            	<field attributeName="proposalNumber" forceInquiry="false"/>
				<field attributeName="application" forceInquiry="false"/>				
            </resultFields>
        </lookup>
		
		<attributes>        
			<attribute name="proposalNumber">
				<label>Proposal Number </label>
				<shortLabel>Proposal Number </shortLabel>
				<maxLength>8</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<hidden />
				</control>
				<summary>Proposal Number </summary>
				<description>Proposal Number </description>
			</attribute>
			
			<attribute name="application">
				<label>Application </label>
				<shortLabel>Application</shortLabel>
				<maxLength>4000</maxLength>
				<validationPattern><numeric /></validationPattern>
				<control>
					<hidden />
				</control>
				<summary>Application</summary>
				<description>Application</description>
			</attribute>			
		
		</attributes>
    </businessObject>

</dictionaryEntry>
        