<!DOCTYPE dictionaryEntry PUBLIC
    "-//Kuali Project//DTD Data Dictionary 1.0//EN"
    "dataDictionary-1_0.dtd">
<dictionaryEntry>
    <businessObject businessObjectClass="org.kuali.kra.proposaldevelopment.bo.ProposalPersonBiography">

        <inquiry>
            <title>Proposal Person Biography</title>

            <inquirySections><inquirySection title="Prop Person Bio"><inquiryFields>
            	<field attributeName="proposalPersonNumber" forceInquiry="true"/>
            	<field attributeName="biographyNumber" forceInquiry="true"/>
				<field attributeName="personId" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="description"/>
				<field attributeName="documentTypeCode"/>

            </inquiryFields></inquirySection></inquirySections>
        </inquiry>

        <lookup>
            <title>Proposal Person Biography Lookup</title>
			<menubar>
				<![CDATA[<a href="index.jsp">Main</a>]]>
			</menubar>
            <instructions>Look up a Prop Person Bio</instructions>

            <defaultSort sortAscending="true">
                <sortAttributes>
                	<sortAttribute attributeName="proposalPersonNumber"/>
                	<sortAttribute attributeName="biographyNumber"/>
					<sortAttribute attributeName="personId"/>
					<sortAttribute attributeName="proposalNumber"/>

                </sortAttributes>
            </defaultSort>

            <lookupFields>
            	<lookupField attributeName="proposalPersonNumber" required="false"/>
            	<lookupField attributeName="biographyNumber" required="false"/>
				<lookupField attributeName="personId" required="false"/>
				<lookupField attributeName="proposalNumber" required="false"/>
				<lookupField attributeName="description" required="false"/>
				<lookupField attributeName="documentTypeCode" required="false"/>

            </lookupFields>

            <resultFields>
            	<field attributeName="proposalPersonNumber" forceInquiry="true"/>
            	<field attributeName="biographyNumber" forceInquiry="true"/>
				<field attributeName="personId" forceInquiry="true"/>
				<field attributeName="proposalNumber" forceInquiry="true"/>
				<field attributeName="description" forceInquiry="false"/>
				<field attributeName="documentTypeCode" forceInquiry="false"/>

            </resultFields>
        </lookup>

       <!--  can't have multiple titleattribute 
        <titleAttribute>biographyNumber</titleAttribute>
		<titleAttribute>personId</titleAttribute>
		-->
		<titleAttribute>proposalNumber</titleAttribute>
        <attributes>

	        <attribute name="proposalPersonNumber">
			<label>Person </label>
			<shortLabel>Person </shortLabel>
			<maxLength>12</maxLength>
			<validationPattern><numeric /></validationPattern>
			<required> true </required> 
			<control>
				<text size="12" />
			</control>
			<summary>Proposal Person Number </summary>
			<description>This field is auto-populated with the names of the individuals listed in the key personnel section of the proposal.</description>
			</attribute>
	        <attribute name="biographyNumber">
			<label>Biography Number </label>
			<shortLabel>Biography Number </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="3" />
			</control>
			<summary>Biographt Number </summary>
			<description>Biography Number </description>
			</attribute>

			<attribute name="personId">
			<label>Person Id </label>
			<shortLabel>Person Id </shortLabel>
			<maxLength>9</maxLength>
			<validationPattern><alphaNumeric /></validationPattern>
			<control>
				<text size="9" />
			</control>
			<summary>Person Id </summary>
			<description>Person Id </description>
			</attribute>
			<attribute name="rolodexId">
			<label>Rolodex Id </label>
			<shortLabel>Rolodex Id </shortLabel>
			<maxLength>6</maxLength>
			<validationPattern><numeric /></validationPattern>
			<control>
				<text size="6" />
			</control>
			<summary>Rolodex Id </summary>
			<description>Rolodex Id </description>
			</attribute>

			<attribute name="proposalNumber">
			<label>Proposal Number </label>
			<shortLabel>Proposal Number </shortLabel>
				<maxLength>12</maxLength>
				<validationPattern>
					<numeric />
				</validationPattern>
				<control>
					<text size="12" />
				</control>
			<summary>Proposal Number </summary>
			<description>The unique proposal number identifying a proposal. This is a system generated, sequential number. </description>
			</attribute>

			<attribute name="description">
			<label>Description </label>
			<shortLabel>Description </shortLabel>
			<maxLength>200</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true"/></validationPattern>
			<control>
				<textarea cols="20" rows="3"  />
			</control>
			<summary>Description </summary>
			<description>Description </description>
			</attribute>

			<attribute name="documentTypeCode">
			<label>Attachment Type  </label>
			<shortLabel>Attachment Type </shortLabel>
			<maxLength>3</maxLength>
			<validationPattern><numeric /></validationPattern>
			<required> true </required> 
			<control>                    
                    <select valuesFinderClass="org.kuali.kra.lookup.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" businessObjectClass="org.kuali.kra.bo.PropPerDocType" keyAttribute="documentTypeCode" labelAttribute="description" includeKeyInLabel="false"/>
			</control>
			<summary>Document Type Code </summary>
			<description>This field contains a list of personnel attachment types </description>
			</attribute>
            <attributeReference name="updateUser" sourceClassName="org.kuali.kra.bo.KraAttributeReferenceDummy" sourceAttributeName="updateUser" >
				<label>Uploaded By</label>
				<shortLabel>Uploaded By</shortLabel>
				<summary>User who uploads the document</summary>
				<description>User who uploads the document</description>
			</attributeReference>
			<attributeReference name="updateTimestamp" sourceClassName="org.kuali.core.bo.AttributeReferenceDummy" sourceAttributeName="genericDate" >
				<label>Posted Timestamp</label>
				<shortLabel>Posted Timestamp</shortLabel>
				<summary>Date and Time when attachments gets uploaded</summary>
				<description>Date and Time when attachments gets uploaded</description>
			</attributeReference>

			<attribute name="propPerDocType.description">
				<label>Attachment Type</label>
				<shortLabel>Attachment Type</shortLabel>

				<maxLength>200</maxLength>
				<validationPattern>
					<anyCharacter  allowWhitespace="true"  />
				</validationPattern>

				<control>
					<text size="200" />
				</control>

				<summary>Prop per doc type description</summary>
				<description>Proposal person document type description</description>
			</attribute>
			<attribute name="fileName">
			<label>File Name </label>
			<shortLabel>File Name </shortLabel>
			<maxLength>150</maxLength>
			<validationPattern><anyCharacter allowWhitespace="true" /></validationPattern>
			<required> true </required> 
			
			<control>
				<text size="150" />
			</control>
			<summary>File Name </summary>
			<description>This is the name of the file path and name that the user is uploading; can be typed in or the user can use the “browse” feature to find the file on their computer or attached mass storage device</description>
			</attribute>

		</attributes>

		
    </businessObject>

</dictionaryEntry>
