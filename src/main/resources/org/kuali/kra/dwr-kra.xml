<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN" "http://www.getahead.ltd.uk/dwr/dwr10.dtd">

<dwr>

	<init>
		<creator id="spring" class="org.kuali.core.web.servlet.dwr.GlobalResourceDelegatingSpringCreator"/>
	</init>
	<allow>
		<convert converter="bean" match="org.kuali.kra.bo.Sponsor">
			<param name="include" value="sponsorCode,sponsorName" />
		</convert>
		<create creator="spring" javascript="ProposalDevelopmentService">
			<param name="beanName" value="proposalDevelopmentService" />
			<include method="getSponsorCode" />
		</create>
	</allow>
</dwr>