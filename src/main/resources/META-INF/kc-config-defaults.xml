<config>
	
	<!--  from kc-config.xml start-->
	<param name="portal.show.sample.app">true</param>
	
	<param name="environment">${build.environment}</param>
	<param name="build.environment" override="false">dev</param>
	<param name="version">${build.version}</param>

	<param name="module.name">kc</param>
