	<!-- possible options are, T for Test and P for Production-->
	<param name="edu.coeus.mail.mode">T</param>
	<param name="edu.coeus.mail.testReceiverId">kc.grants.gov@kuali.org</param>

	<param name="s2s.keystore.location">path to keystore</param>
	<param name="s2s.keystore.password">keystore password</param>
