					<property name="actionLabel" value="Select"/>
					<property name="methodToCall" value="selectLine" />
					<property name="successCallback" value="dismissDialog('PropDev-performanceSitesPage-Wizard');" />				
					<property name="refreshId" value="PropDev-PerformanceSitesPage-Section" />
				</bean>
			</list>
		</property>
