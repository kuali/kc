				<bean parent="Uif-DataField" p:propertyName="addressLine2" />
				<bean parent="Uif-DataField" p:propertyName="addressLine3" />
				<bean parent="Uif-DataField" p:propertyName="state" />
				<bean parent="Uif-DataField" p:propertyName="city" />
				<bean parent="Uif-DataField" p:propertyName="postalCode" />
