		<property name="headerText" value="Search Results" />
		<property name="additionalCssClasses" value="kc-personnel-table" />
		<property name="collectionObjectClass" value="org.kuali.coeus.common.framework.rolodex.Rolodex" />
		<property name="propertyName" value="addOrganizationHelper.results" />
