<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="PersonEditableFieldMaintenanceDocument" parent="PersonEditableFieldMaintenanceDocument-parentBean"/>

  <bean id="PersonEditableFieldMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.coeus.common.framework.person.attr.PersonEditableField"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="PersonEditableFieldMaintenanceDocument-SpecifytheFieldName"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>personEditableFieldId</value>
      </list>
    </property>	<property name="allowsRecordDeletion" value="true" />	
    
    
    
    
    
    <property name="documentTypeName" value="PersonEditableFieldMaintenanceDocument"/>
    <property name="businessRulesClass" value="org.kuali.kra.proposaldevelopment.rules.PersonEditableFieldRule"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    
    
  </bean>

<!-- Maintenance Section Definitions -->


  <bean id="PersonEditableFieldMaintenanceDocument-SpecifytheFieldName" parent="PersonEditableFieldMaintenanceDocument-SpecifytheFieldName-parentBean"/>

  <bean id="PersonEditableFieldMaintenanceDocument-SpecifytheFieldName-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Specify the Field Name"/>
    <property name="title" value="Specify the Field Name"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="moduleCode" p:noLookup="true" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="fieldName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="active"/>
        <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
      </list>
    </property>
  </bean>
</beans>