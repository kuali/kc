<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="IacucProtocolSubmissionQualifierTypeMaintenanceDocument" parent="IacucProtocolSubmissionQualifierTypeMaintenanceDocument-parentBean"/>
	<bean id="IacucProtocolSubmissionQualifierTypeMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.iacuc.actions.submit.IacucProtocolSubmissionQualifierType"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="IacucProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType"/>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="false" />
		<property name="documentTypeName" value="IacucProtocolSubmissionQualifierTypeMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="IacucProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType" parent="IacucProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType-parentBean"/>
	<bean id="IacucProtocolSubmissionQualifierTypeMaintenanceDocument-EditProtocolSubmissionQualifierType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit Protocol Submission Type Qualifier"/>
		<property name="title" value="Edit IACUC Protocol Submission Type Qualifier"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="submissionQualifierTypeCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
			</list>
		</property>
	</bean>
</beans>
