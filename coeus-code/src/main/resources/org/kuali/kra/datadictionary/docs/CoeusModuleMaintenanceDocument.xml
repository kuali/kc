<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="CoeusModuleMaintenanceDocument" parent="CoeusModuleMaintenanceDocument-parentBean" />
    <bean id="CoeusModuleMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.common.framework.module.CoeusModule" />
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
        <property name="maintainableSections" >
            <list>
                <ref bean="CoeusModuleMaintenanceDocument-EditCoeusModule" />
            </list>
        </property>
        <property name="lockingKeys" >
            <list>
                <value>moduleCode</value>
            </list>
        </property>
        <property name="allowsRecordDeletion" value="true" />
        <property name="documentTypeName" value="CoeusModuleMaintenanceDocument" />
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase" />
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="CoeusModuleMaintenanceDocument-EditCoeusModule" parent="CoeusModuleMaintenanceDocument-EditCoeusModule-parentBean" />
    <bean id="CoeusModuleMaintenanceDocument-EditCoeusModule-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Coeus Module" />
        <property name="title" value="Edit Coeus Module" />
        <property name="maintainableItems" >
            <list>
                <bean parent="MaintainableFieldDefinition"
                    p:name="moduleCode"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="description"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
        </property>
    </bean>
</beans>
