              <bean p:propertyName="moduleTitle" parent="Uif-DataField"/>
              <bean p:propertyName="narrativeTypeCode" parent="Uif-DataField"/>
              <bean p:propertyName="phoneNumber" parent="Uif-DataField"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

