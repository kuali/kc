<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="NegotiationCustomData" parent="NegotiationCustomData-parentBean" />

  <bean id="NegotiationCustomData-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.negotiations.customdata.NegotiationCustomData" />
		<property name="objectLabel" value="Negotiation Custom Data" />
    <property name="titleAttribute" value="negotiationCustomData" />
    <property name="helpDefinition" >
      <bean parent="HelpDefinition"
            p:parameterNamespace="KC-Negotiation"
            p:parameterName="negotiationCustomDataHelpUrl"
            p:parameterDetailType="Document" />
    </property>
    <property name="attributes" >
      <list>
        <ref bean="NegotiationCustomData-value" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="NegotiationCustomData-value" parent="NegotiationCustomData-value-parentBean" />

  <bean id="NegotiationCustomData-value-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="value" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Value" />
    <property name="shortLabel" value="Value" />
    <property name="maxLength" value="2000" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="60" />
    </property>
    <property name="summary" value="Value" />
    <property name="description" value="Value" />
  </bean>
</beans>
