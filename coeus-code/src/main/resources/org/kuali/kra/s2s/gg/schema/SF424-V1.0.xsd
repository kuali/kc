        <xsd:documentation>SF424: Areas Affected By Project</xsd:documentation>
        <xsd:documentation>TS194 v4020: No matching data element</xsd:documentation>
    </xsd:annotation>
    <xsd:element name="NamePrefix" type="glob:StringMin1Max10Type"/>
