<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false">
  <local-check-config name="KC Coding Standards" location="src/main/config/KC_Checkstyle.xml" type="project" description="">
    <property name="cs.suppression.file" value="${basedir}/src/main/config/checkstyle_suppressions.xml"/>
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="KC Coding Standards" local="true">
    <file-match-pattern match-pattern="^src/main/java.*|src/test/java.*" include-pattern="true"/>
  </fileset>
</fileset-config>
